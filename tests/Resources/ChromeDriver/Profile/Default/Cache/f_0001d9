define('carousel/global.ux.carousel',['jquery', 'handlebars', 'hammer', 'personalisation/global.ux.personalise', 'responsive/global.ux.imageInterchange'], function($, handlebars, Hammer, guxPersonalise, guxImageInterchange) {
var props = {
'carouselInitialised' : false,
'self' : null,
'carouselContainer': null,
'carousels' : [],
'csstransforms3d' : true,
'currentPosition' : 0,
'selectedPosition' : 0,
'options' : {
'controls' : true,
'pagination' : true,
'autoMode' : true,
'autoDelayTime' : 3,
'autoDirection' : 'next'
},
'element' : {
'slideElements' : ''
},
'count' : {
'slideElements' : 0,
'autoSlide' : 0
},
'position' : {
'current' : 1
},
'widths' : {
'li' : 0,
'ul' : 0
},
'animation' : {
'speed' : 0,
'delay' : 0,
'distance' : 0,
'movement' : 0,
'transistionStatus' : false,
'autoSlideTimer' : null,
'gestureTimer' : null,
'dragState' : false,
'dragScrollState' : false,
'pagination' : false
},
'lastViewedVehicle' : {},
'omnitureValues' : {
'pageName' : '',
'data' : {
'deviceType' : 'mobile',
'cmTemplate' : 'new',
'cmAction' : '',
'cmContentType' : '',
'cmContentId' : ''
}
}
};
/* Todo:  
- still to do:
- jquery animation fallback for non 3d animation browsers (ie8, ie9, etc)
- slider doesn't work with more than one carousel in place, need to find a solution for this, so it can work with multiple sliders on
- need to implement a parent array stucture where each carousel has its own index position in its individual array which contains it's necessary components
needed in order to function correctly, when loopig over each slider, store a specific index to identify that slider being worked on
- carousel module needs to be fully object orientated, currently it is not
*/
var carouselSetup = function(){
/* css3 animation support */
if(!Modernizr.csstransforms3d){
props.csstransforms3d = false;
//console.log('csstransforms3d not supported!');
}
//find all sliders on the page
props.carouselContainer = $('.gux-slider');
var elementlength = props.carouselContainer.find('li').length;
/* only add carousel features on billboard if more than one */
if (elementlength > 1) {
setOptions();
htmlCarouselSetup();
$(window).resize(function(){
calcWidths();
});
calcWidths();
props.carouselContainer.removeClass('loading');
controlsSetup();
paginationSetup();
animationSetup();
setupTouch();
props.carouselInitialised = true;
/* calling image interchange module */
guxImageInterchange.captureImageData();
} else {
//console.log('ONLY ONE IMAGE ASSET EXISTS!');
props.carouselContainer.removeClass('loading');
}
};
/* setting the authorable options set by the author from the html markup */
var setOptions = function(){
//console.log('FUNCTION:setOptions');
var paginationState = props.carouselContainer.data('pagination'),
autoModeState = props.carouselContainer.data('automode'),
autoDelayTime = props.carouselContainer.data('delay');
props.options.autoDirection = props.carouselContainer.data('autodirection');
if (paginationState === false){
props.options.pagination = false;
}
if (autoModeState === false){
props.options.autoMode = false;
}
if (autoDelayTime){
props.options.autoDelayTime = 1000 * parseInt(props.carouselContainer.data('delay'),10);
}
};
var htmlCarouselSetup = function (){
//console.log('FUNCTION:htmlCarouselSetup');
var carouselHtmlMarkup = {},
$ulElement,
$liElements,
$carouselArrows,
$carouselPagination;
/* cloning the carousel first and last slides */
var cloneSlides = function (){
//console.log('FUNCTION:cloneslides');
$ulElement = props.carouselContainer.find('ul');
$liElements = $ulElement.find('li');
/* cloning first and last LI elements, adding clone and slide classes and data attributes to the cloned elements */
props.carouselContainer.find('li').last().clone().prependTo($ulElement).addClass('clone').attr('data-clone', 'last')
.siblings('li').first().clone().appendTo($ulElement).addClass('clone').attr('data-clone', 'first')
.parent().find('li').addClass('slides');
};
var populateSlideElementInfo = function (){
//console.log('FUNCTION:populateSlideElementInfo');
props.element.slideElements = props.carouselContainer.find('li').not('.clone');
props.count.slideElements = props.element.slideElements.length;
};
var setupPaginationHTML = function () {
//console.log('FUNCTION:setupPaginationHTML');
var activeClass = '';
carouselHtmlMarkup.paginationControls = '<ol class="pagination">';
for (var i = 1; i <= props.count.slideElements; i++){
if (i === 1){
activeClass = 'active-pagination';
} else {
activeClass = '';
}
carouselHtmlMarkup.paginationControls += '<li class="' + activeClass + '" data-index="' + i + '"><a class="controls" role="button">' + i +'</a></li>';
}
carouselHtmlMarkup.paginationControls += '</ol>';
$carouselPagination = props.carouselContainer.find('.carousel-pagination');
$carouselPagination.append(carouselHtmlMarkup.paginationControls);
};
var setIndexes = function(){
//console.log('FUNCTION:setIndexes');
var $this;
props.carouselContainer.find('ul li').not('.clone').each(function(index){
$this = $(this);
if (index === 0){
$this.addClass('active-slide');
}
$this.attr('data-index', index+1);
});
};
cloneSlides();
populateSlideElementInfo();
if (props.count.slideElements > 1){
}
if (props.options.pagination) {
setupPaginationHTML();
}
if (props.options.autoMode) {
props.animation.autoSlideTimer = setTimeout(function(){autoSlide();}, props.options.autoDelayTime);
}
setIndexes();
};
/* calculating the width of the current viewport, adding widths to li elements */
var calcWidths = function (){
//console.log('FUNCTION:calcWidths');
var noLiElements = props.carouselContainer.find('ul li').length;
props.widths.li = props.carouselContainer.width();
props.widths.ul = props.widths.li * noLiElements;
/* measurement between each slide (in pixels) */
props.animation.distance = props.widths.li;
/* setting the index of the current active slide */
props.position.current = props.carouselContainer.find('li.active-slide').data('index');
props.carouselContainer.find('ul').width(props.widths.ul)
.children('li').width(props.widths.li);
/* setting the movement value to the position of the current active slide */
if (props.position.current > 1){
props.animation.movement =  props.animation.distance * props.position.current;
} else {
props.animation.movement = props.animation.distance;
}
props.carouselContainer.find('ul').css({transition: '0s', transform: 'translateX(-' + props.animation.movement + 'px)'});
};
var paginationSetup = function(){
//console.log('FUNCTION:paginationSetup');
if (!props.options.pagination){
//console.log('PAGINATION CONTROLS ON');
} else {
//console.log('PAGINATION CONTROLS OFF');
}
};
var animationSetup = function (){
//console.log('FUNCTION:animationSetup');
// getting animation settings from DOM
props.animation.speed = props.carouselContainer.data('speed');
props.animation.delay = props.carouselContainer.data('delay');
if (props.csstransforms3d){
props.carouselContainer.find('ul').css({transition: '0s',transform: 'translateX(-' + props.animation.distance + 'px)'});
} else {
/* animation jQuery for older browsers */
}
};
var controlsSetup = function(){
//console.log('FUNCTION:controlsSetup');
var $this,
$element,
$slideliElments,
$liElement,
$anchorTag,
$imgTag;
props.carouselContainer.find('a.controls').each(function(){
$this = $(this);
$this.on('click', function(e){
e.preventDefault();
e.stopPropagation();
stopAutoSlide();
if(!props.transistionStatus){
$element = $(this);
animationController($element);
}
});
});
//omniture
$slideliElments = props.carouselContainer.find('li');
$slideliElments.each(function(){
$liElement = $(this);
$liElement.find('.heading a').on('click', function(e){
e.stopPropagation();
$anchorTag = $(this);
props.omnitureValues.data.cmAction = 'carousel cta click';
props.omnitureValues.data.cmContentId = $anchorTag.closest('li').find('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
});
});
};
var animationController = function ($element){
//console.log('FUNCTION:animationController');
var $this = $element,
$currentActiveSlide;
if ($this.hasClass('next')){
//console.log('next button clicked');
//omniture
props.omnitureValues.data.cmAction = 'carousel scroll next';
props.omnitureValues.data.cmContentId = $this.closest('.gux-slider-wrapper').find('li.active-slide').children('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
animateNext();
} else if ($this.hasClass('prev')){
//console.log('prev button clicked');
//omniture
props.omnitureValues.data.cmAction = 'carousel scroll previous';
props.omnitureValues.data.cmContentId = $this.closest('.gux-slider-wrapper').find('li.active-slide').children('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
animatePrev();
}  else if ($this.closest('ol').hasClass('pagination')){
//console.log('pagination button clicked');
//omniture
props.omnitureValues.data.cmAction = 'carousel pagination click';
props.omnitureValues.data.cmContentId = $this.closest('.gux-slider-wrapper').find('li.active-slide').children('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
// disabling pagination click feature
var currentPaginationIndex = $this.closest('ol').find('.active-pagination').attr('data-index');
var selectedPaginationIndex = $this.closest('li').attr('data-index');
var paginationDifference;
var $liElements = props.carouselContainer.find('ul li').not('.clone');
$this.closest('li').addClass('active-pagination').siblings('li').removeClass('active-pagination');
//console.log('currentPaginationIndex = ' + currentPaginationIndex);
//console.log('selectedPaginationIndex = ' + selectedPaginationIndex);
if (currentPaginationIndex !== selectedPaginationIndex){
paginationDifference = selectedPaginationIndex - currentPaginationIndex;
//console.log('paginationDifference = ' + paginationDifference);
props.animation.movement = props.animation.distance * selectedPaginationIndex;
if (paginationDifference > 0) {
//console.log('direction = forwards!');
//console.log('props.animation.movement = ' + props.animation.movement);
$liElements.removeClass('active-slide');
$liElements.eq(selectedPaginationIndex-1).addClass('active-slide');
props.position.current = props.carouselContainer.find('ul li.active-slide').attr('data-index');
//console.log('props.position.current = ' + props.position.current);
animationMovement();
props.animation.pagination = true;
} else {
//console.log('direction = backwards!');
//console.log('props.animation.movement = ' + props.animation.movement);
$liElements.removeClass('active-slide');
$liElements.eq(selectedPaginationIndex-1).addClass('active-slide');
props.position.current = props.carouselContainer.find('ul li.active-slide').attr('data-index');
//console.log('props.position.current = ' + props.position.current);
animationMovement();
props.animation.pagination = true;
}
} else {
//console.log('clicked on the same item!');
}
} else {
//console.log('not a registered click element');
}
};
var animateNext = function(){
//console.log('FUNCTION:animateNext');
props.animation.movement += props.animation.distance;
props.carouselContainer.find('li.active-slide').removeClass('active-slide').next().addClass('active-slide');
props.animation.pagination = false;
animationAction();
};
var animatePrev = function(){
//console.log('FUNCTION:animatePrev');
props.animation.movement -= props.animation.distance;
props.carouselContainer.find('li.active-slide').removeClass('active-slide').prev().addClass('active-slide');
props.animation.pagination = false;
animationAction();
};
var animationAction = function(){
//console.log('FUNCTION:animationAction');
props.transistionStatus = true;
animationMovement();
var $currentActiveSlide = props.carouselContainer.find('li.active-slide');
var clonePosition = cloneCheck($currentActiveSlide);
//console.log('clonePosition = ' + clonePosition);
props.carouselContainer.find('ul').one("transitionend webkitTransitionEnd", function() {
//console.log('props.carouselContainer.find(ul) = ');
//console.log(props.carouselContainer.find('ul'));
if (!props.animation.pagination) {
if (clonePosition !== '') {
//console.log('ENTERED CLONED POSITION!');
//console.log('clonePosition = ' + clonePosition);
cloneSlideSwitch($currentActiveSlide, clonePosition);
props.position.current = props.carouselContainer.find('li.active-slide').index();
//console.log('props.position.current INDEX = ' + props.position.current);
updateActivePagination();
} else {
//console.log('NOT A CLONE!');
props.position.current = props.carouselContainer.find('li.active-slide').index();
//console.log('props.position.current INDEX = ' + props.position.current);
updateActivePagination();
}
clearTimeout(props.animation.gestureTimer);
props.transistionStatus = false;
props.animation.dragState = false;
props.animation.dragScrollState = false;
} else {
//console.log('action not performed!');
}
});
};
var updateActivePagination = function(){
//console.log('FUNCTION:updateActivePagination');
props.carouselContainer.find('ol li.active-pagination').removeClass('active-pagination');
var $paginationLi = props.carouselContainer.find('ol li');
$paginationLi.each(function(){
if ($(this).data('index') == props.position.current) {
$(this).addClass('active-pagination');
}
});
};
var animationMovement = function(){
//console.log('FUNCTION:animationMovement');
if (props.csstransforms3d){
props.carouselContainer.find('ul').css({transition: props.animation.speed +'s', transform: 'translateX(-' + props.animation.movement + 'px)'});
} else {
/* fallback animation for browsers that do not support tranform css3 animation */
}
};
/* checking if slide is a clone and returning its position */
var cloneCheck = function ($currentActiveSlide){
//console.log('FUNCTION:cloneCheck');
var cloneType = '';
if ($currentActiveSlide.hasClass('clone')){
cloneType = $currentActiveSlide.data('clone');
return cloneType;
} else {
return cloneType;
}
};
var cloneSlideSwitch = function($currentActiveSlide, clonePosition) {
//console.log('FUNCTION:cloneSlideSwitch');
var $cloneSlideELement;
// disabling swipe gestures while clone switch occurs
props.animation.dragState = true;
props.animation.dragScrollState = true;
//console.log('clonePosition = ' + clonePosition);
//console.log('props.animation.distance = ' + props.animation.distance);
var cloneSwitch = function($cloneSlideELement) {
//console.log('FUNCTION:cloneSwitch');
props.animation.movement = props.animation.distance * $cloneSlideELement.data('index');
//console.log('props.animation.distance = ' + props.animation.distance);
//console.log('$cloneSlideELement.data(index) = ' + $cloneSlideELement.data('index'));
//console.log('props.animation.movement = ' + props.animation.distance);
if (props.csstransforms3d){
$currentActiveSlide.closest('ul').css({transition: '0s', transform: 'translateX(-' + props.animation.movement + 'px)'});
$cloneSlideELement.addClass('active-slide').siblings('li').removeClass('active-slide');
} else {
/* fallback animation for browsers that do not support tranform css3 animation */
}
};
if (clonePosition === 'first') {
//console.log('FIRST CLONE!');
$cloneSlideELement = props.element.slideElements.first();
cloneSwitch($cloneSlideELement);
} else if (clonePosition === 'last') {
//console.log('LAST CLONE!');
$cloneSlideELement = props.element.slideElements.last();
cloneSwitch($cloneSlideELement);
}
//enabling swipe gestures after clone switch ends
props.animation.dragState = false;
props.animation.dragScrollState = false;
};
var autoSlide = function(){
//console.log('props.count.slideElements = ' + props.count.slideElements);
if (props.count.autoSlide < props.count.slideElements) {
if (props.options.autoDirection == 'next'){
animateNext();
} else if (props.options.autoDirection == 'prev'){
animatePrev();
}
props.animation.autoSlideTimer = setTimeout(function(){autoSlide();}, props.options.autoDelayTime);
props.count.autoSlide++;
} else {
stopAutoSlide();
}
};
var stopAutoSlide = function(){
//console.log('FUNCTION:stopAutoSlide');
if (props.options.autoMode !== false){
clearTimeout(props.animation.autoSlideTimer);
}
};
var setupTouch = function(){
//console.log('FUNCTION:setupTouch');
var guxCarousel = $('.gux-slider ul')[0]; // using jquery to return dom object
//console.log(guxCarousel);
var direction;
var options = {
dragLockToAxis: true,
dragBlockHorizontal: true
};
var swipeGesture = new Hammer(guxCarousel, options);
swipeGesture.on("dragright dragleft swiperight swipeleft", function(event) {
props.animation.gestureTimer = setTimeout(function(){
props.animation.dragState = false;
props.transistionStatus = false;
}, 1000);
//console.log(event);
event.preventDefault();
event.gesture.preventDefault();
if (!props.animation.dragState && !props.transistionStatus){
stopAutoSlide();
direction = event.gesture.direction;
if (direction == 'left'){
//omniture
props.omnitureValues.data.cmAction = 'carousel swipe left';
props.omnitureValues.data.cmContentId = $(this).find('li.active-slide').children('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
props.animation.dragState = true;
props.animation.dragScrollState = true;
animateNext();
event.gesture.stopDetect();
} else if(direction == 'right'){
//omniture
props.omnitureValues.data.cmAction = 'carousel swipe right';
props.omnitureValues.data.cmContentId = $(this).find('li.active-slide').children('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
props.animation.dragState = true;
props.animation.dragScrollState = true;
animatePrev();
event.gesture.stopDetect();
} else {
//console.log('DIRECTION UP OR DOWN!');
}
}
});
swipeGesture.on("dragdown dragup swipedown swipeup", function(event) {
if (props.animation.dragScrollState === true) {
//console.log('SCROLL STOPPED - ANIMATION IS RUNNING!');
event.gesture.preventDefault();
} else {
props.animation.dragState = true;
}
//console.log('GESTURE = ');
//console.log(event.gesture);
});
// zoom in/zoom out gesture
swipeGesture.on("transform", function(event) {
//console.log('tranfrom EVENT!');
//console.log(event);
stopAutoSlide();
});
swipeGesture.on("release", function(event) {
if (event.gesture.eventType == 'release' || event.gesture.eventType == 'end') {
//console.log('RELEASE EVENT!');
props.animation.dragState = false;
props.animation.dragScrollState = false;
}
});
};
var personaliseCarouselHTML = function(data){
/*console.log('FUNCTION:personaliseCarouselHTML');*/
/*
Handlebars.registerHelper('log', function(text) {
return console.log(text);
});
*/
// format: {{log this}}
//props.lastViewedVehicle = data.fpsReturnedData.RecentlyViewedVehicles[0];
//console.log(props.lastViewedVehicle);
var siteName;
props.lastViewedVehicle = data.fpsReturnedData.LastViewedVehicle[0];
if (typeof(GuxGlobals) !== 'undefined') {
siteName = GuxGlobals.site;
}
var url = window.location.hostname === 'localhost' ? '/assets/js/jsondata/vehicleData.json' : '/cs/ContentServer?pagename=GlobalUX/GetBillboardData&type=personalised&site='+siteName;
var matchFound = false,
dataIndex,
$this;
var vehicleDataRequest = $.getJSON( url, function(data) {
if (typeof(data.billboards) !== 'undefined'){
/*
console.log('SUCCESS ON CONTENT JSON LOAD!');
console.log(data.billboards);
*/
for (var i = 0; i < data.billboards.length; i++){
if (data.billboards[i].name == props.lastViewedVehicle._name){
dataIndex = i;
matchFound = true;
break;
}
}
if (matchFound) {
var vehicleData = data.billboards[dataIndex];
//console.log('VEHICLE DATA = ');
//console.log(vehicleData);
var source = $("#carousel-template").html();
var template = Handlebars.compile(source);
$('.gux-slider ul > li').each(function(){
$this = $(this);
if ($this.hasClass('personalised')) {
//console.log('REMOVED PERSONALISED ELEMENT!');
$this.remove();
}
});
$('.gux-slider ul').prepend(template(vehicleData));
guxImageInterchange.captureImageData();
carouselSetup();
} else {
//console.log('NO MATCH FOUND IN JSON RETRIEVED CONTENT SERVICE');
carouselSetup();
}
} else {
//console.log('BILLBOARD DATA DOESNT EXIST IN RESPONSE');
carouselSetup();
}
})
.done(function() {
//console.log( "second success" );
})
.fail(function() {
//console.log( "error" );
// IF ERROR INITIALISE CAROUSEL AS DEFAULT
carouselSetup();
});
/*
vehicleDataRequest.complete(function(data) {
//console.log( "second complete" );
});
*/
};
return {
init: function() {
// omniture
props.omnitureValues.pageName = $('#global-ux').data('page');
props.omnitureValues.data.cmTemplate = 'new';
props.omnitureValues.data.cmContentType = 'billboard';
guxPersonalise.init().done(function(data){
//console.log('DATA COLLECTED FROM GUX PERSONALIZATION MODULE - COLLECTED!');
//console.log(data);
/*
console.log('PERSONALISED CAROUSEL INITIALISED!');
console.log('data = ');
console.log(data);
console.log('data.fpsReturnedData.LastViewedVehicle = ');
console.log(data.fpsReturnedData.LastViewedVehicle);
*/
props.lastViewedVehicle = data.fpsReturnedData.LastViewedVehicle;
if (typeof(data.fpsReturnedData.LastViewedVehicle) !== 'undefined') {
if (data.fpsReturnedData.LastViewedVehicle.length > 0){
//console.log('RESPONSE HAS DATA!');
// omniture
props.omnitureValues.data.cmTemplate = 'return';
personaliseCarouselHTML(data);
} else {
//console.log('RESPONSE HAS NO DATA!');
carouselSetup();
}
} else {
//console.log('RESPONSE VALUES CANNOT BE READ!');
carouselSetup();
}
})
.fail(function(){
//console.log('guxPersonalise REQUEST FAILED!');
carouselSetup();
});
},
status: function(){
return props.carouselInitialised;
},
stopAutoSlide : stopAutoSlide
};
});
define('showroom/global.ux.showroom',['jquery'], function($) {
var props = {
'container' : null,
'omnitureValues' : {
'pageName' : '',
'data' : {
'deviceType' : 'mobile',
'cmTemplate' : 'unknown',
'cmAction' : '',
'cmContentType' : '',
'cmContentId' : ''
}
}
};
var showroomSetup = function(){
var $this,
$ctaLink;
// omniture
props.omnitureValues.pageName = $('#global-ux').data('page');
props.omnitureValues.data.cmContentType = 'showroom';
props.container = $('.showroom');
props.container.each(function(){
$this = $(this);
$this.find('.heading a').on('click', function(e){
e.stopPropagation();
$ctaLink = $(this);
props.omnitureValues.data.cmTemplate = GuxGlobals.omniturePersonalisedState;
props.omnitureValues.data.cmAction = 'showroom cta click';
props.omnitureValues.data.cmContentId = $ctaLink.closest('.showroom-inner').find('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
});
});
};
return {
init: function() {
//console.log('SHOWROOM MODULE LOADED!');
showroomSetup();
}
};
});
define('promotion/global.ux.promotion',['jquery'], function($) {
var props = {
