$scope.params = $routeParams;
ConfigService.getConfig().then(function() {
$scope.config = ConfigService.config;
$scope.filter.distance = $scope.config.distances.default;
addressFormat = $scope.config.addressformat.split(',');
$scope.dealerlocator.addressFormat = addressFormat;
if($scope.config.premiumFilter) {
$scope.config.premiumFilter.selected = checkMustang();
$scope.$watch(function(scope) {
return scope.config.premiumFilter.selected;
}, function() {
$scope.sizeElements();
});
}
});
$scope.desktop = window.matchMedia('(min-width: 768px)');
for(i = 0; i < polygon.length; i++) {
boundsArray.push({
lat: polygon[i].split('+')[1],
lng: polygon[i].split('+')[0]
});
}
$scope.api = new ApiService({
language: GuxGlobals.siteSetup.Locale,
countryCode: GuxGlobals.siteSetup.EcatMarketCode,
//geocodeRegion: GuxGlobals.ccTLD,
countryCodes: GuxGlobals.dlService.countryCodes,
imagePath: imagePath,
countryBounds: boundsArray,
autocompleteCallbackURL: window.location.hostname === 'localhost' ? '/assets/js/dealerlocator/autocomplete.json' : '/cs/ContentServer?pagename=' + GuxGlobals.site + '/GlobalUX/DealerLocatorAutocomplete&callback=autoCompleteDealers',
autocompleteCallbackName: 'autoCompleteDealers'
});
$scope.dealerlocator = {};
$scope.dealerlocator.errorMessage = null;
$scope.dealerlocator.displaySplash = false;
$scope.dealerlocator.searchType = 'location';
$scope.dealerlocator.searchTypePlaceholders = {};
$scope.dealerlocator.searchTypePlaceholders.location = $('#search-type-location').attr('data-placeholder');
$scope.dealerlocator.searchTypePlaceholders.dealername = $('#search-type-dealername').attr('data-placeholder');
$scope.dealerlocator.map = new $scope.api.map(document.getElementById('map'), {
disableBusinessPOI: true
});
if($scope.desktop.matches && desktopDisplayPolygon) {
for(i = 0; i < desktopDisplayPolygon.length; i++) {
displayBounds.push({
lat: desktopDisplayPolygon[i].split('+')[1],
lng: desktopDisplayPolygon[i].split('+')[0]
});
}
} 
if(!$scope.desktop.matches && mobileDisplayPolygon) {
for(i = 0; i < mobileDisplayPolygon.length; i++) {
displayBounds.push({
lat: mobileDisplayPolygon[i].split('+')[1],
lng: mobileDisplayPolygon[i].split('+')[0]
});
}
}
if(displayBounds.length > 0) {
$scope.sizeElements();
$scope.dealerlocator.map.setBounds(displayBounds);
} else {
$scope.dealerlocator.map.setBounds(boundsArray);
}
$scope.dealerlocator.map.addListener($scope.dealerlocator.map.map, 'click', function(e) {
$scope.dealerlocator.clearAutocomplete();
$scope.dealerlocator.resultsVisible = false;
$scope.dealerlocator.resultsAlwaysVisible = false;
$scope.$apply();
});
$scope.dealerlocator.map.addListener($scope.dealerlocator.map.map, 'dragstart', function(e) {
$scope.dealerlocator.clearAutocomplete();
$scope.dealerlocator.resultsVisible = true;
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.$apply();
$scope.dealerlocator.resultsVisible = false;
$scope.dealerlocator.resultsAlwaysVisible = false;
$scope.$apply();
});
$scope.dealerlocator.resultsVisible = true;
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.filterSettings = {};
$scope.filterSettings.hidden = true;
$scope.filters = {};
$scope.offset = $scope.desktop.matches ? $('.dl-search').width() * -0.5 : null;
$scope.page = 0;
$scope.dealerlocator.suggestions = {};
$scope.desktop.addListener(function(media) {
if(media.matches) {
$scope.offset = $('.dl-search').width() * -0.5;
} else {
$scope.offset = null;
}
});
$(window).on('resize', function() {
$scope.sizeElements();
$scope.$apply();
});
$scope.sizeElements();
$('.loading').removeClass('loading');
})
.controller('DealerSearchController', function($scope, $location, $window, ConfigService) {
$scope.search = function(e) {
$scope.dealerlocator.isSearchEnabled =true;
clearTimeout($scope.dealerlocator.map.autocompleteTimeout);
e.preventDefault();
if($scope.searchInput) {
$('#search-field').blur();
if($scope.config.premiumFilter && $scope.config.premiumFilter.selected) {
$scope.clearFilters();
$scope.config.premiumFilter.selected = true;
} else {
$scope.clearFilters();
}
$scope.dealerlocator.suggestions = null;
$scope.dealerlocator.disambiguationLatLng = null;
$location.path('/search/' + encodeSlash($scope.searchInput));
$scope.sizeElements();
} else {
$scope.dealerlocator.errorMessage = $('.error-empty-search').text();
$scope.sizeElements();
var props = {
errorType: 'empty entry',
language: GuxGlobals.siteSetup.OmnitureLanguage,
radius: $scope.filter.distance,
deviceType: 'mobile'
};
if($scope.dealerlocator.searchType === 'location') {
$scope.dealerlocator.currentSearchType = 'location';
props.location = '';
}
if($scope.dealerlocator.searchType === 'dealername') {
$scope.dealerlocator.currentSearchType = 'dealer name';
props.namesearchTerm = '';
}
props.searchSelection = $scope.dealerlocator.currentSearchType;
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
}
};
$scope.$on('$routeChangeStart', function(e, next, current) {
if(next) {
$scope.searchInput = next.pathParams.searchParam;
$scope.dealerlocator.errorMessage = null;
//$scope.dealerlocator.map.deselectMarkers();
}
});
$scope.$on('search-updated', function(e, data) {
$scope.searchInput = data;
});
$scope.locationSearch = function(location, e) {
$scope.dealerlocator.suggestions = null;
$scope.sizeElements();
if($scope.config.premiumFilter && $scope.config.premiumFilter.selected) {
$scope.clearFilters();
$scope.config.premiumFilter.selected = true;
} else {
$scope.clearFilters();
}
$scope.dealerlocator.disambiguationLatLng = null;
$location.path('/search/' + location);
e.preventDefault();
$scope.sizeElements();
};
$scope.dealerSearch = function(dealerID, e) {
$scope.dealerlocator.suggestions = null;
$scope.clearFilters();
$scope.dealerlocator.disambiguationLatLng = null;
$location.path('/dealer/' + dealerID);
e.preventDefault();
$scope.sizeElements();
};
$scope.disambigSearch = function(disambiguation, e) {
$scope.dealerlocator.isSearchEnabled=true;
$scope.dealerlocator.suggestions = null;
$scope.sizeElements();
if($scope.config.premiumFilter && $scope.config.premiumFilter.selected) {
$scope.clearFilters();
$scope.config.premiumFilter.selected = true;
} else {
$scope.clearFilters();
}
$location.path('/search/' + disambiguation.description);
$scope.dealerlocator.disambiguationLatLng = {
lat: disambiguation.lat,
lng: disambiguation.lng
};
e.preventDefault();
$scope.sizeElements();
};
$scope.clearAutocomplete = $scope.dealerlocator.clearAutocomplete = function (){
$scope.dealerlocator.suggestions = {
dealers: [],
locations: []
};
$scope.sizeElements();
};
$scope.searchTypeChange = function() {
$scope.searchInput = '';
$scope.clearAutocomplete();
};
$scope.searchFieldFocus = function() {
if(!$scope.desktop.matches) {
$('body').scrollTop($('.dl-search-type').offset().top);
}
};
$scope.selectPremiumFilter = function() {
$scope.dealerlocator.isSearchEnabled=true;
if($scope.config.premiumFilter && $scope.config.premiumFilter.selected && $scope.dealerlocator.searchType !== 'location') {
$scope.dealerlocator.searchType = 'location';
$scope.searchTypeChange();
}
};
$scope.autocomplete = function() {
if($scope.searchInput.length >= 3) {
$scope.dealerlocator.showDlResultsFilter=false;
if($scope.dealerlocator.searchType === 'location') {
$scope.dealerlocator.errorMessage = null;
$scope.dealerlocator.map.autocomplete($scope.searchInput, 5, function(results) {
if($scope.searchInput.length >= 3) {
$scope.dealerlocator.suggestions = results;
} else {
$scope.dealerlocator.suggestions = {
dealers: [],
locations: []
};
}
$scope.$apply();
$scope.sizeElements();
}, 'locations');
} else {
if($scope.dealerlocator.searchType === 'dealername') {
$scope.dealerlocator.errorMessage = null;
$scope.dealerlocator.map.autocomplete($scope.searchInput, 100, function(results) {
if($scope.searchInput.length >= 3) {
$scope.dealerlocator.suggestions = results;
} else {
$scope.dealerlocator.suggestions = {
dealers: [],
locations: []
};
}
$scope.$apply();
$scope.sizeElements();
}, 'dealers');
}
}
} else {
$scope.clearAutocomplete();
}
};
$scope.useCurrentLocation = function(e) {
$scope.dealerlocator.searchType = 'location';
$scope.dealerlocator.currentSearchType = $scope.dealerlocator.searchType;
$window.navigator.geolocation.getCurrentPosition(function(position) {
$scope.dealerlocator.map.reverseGeocode({lat: position.coords.latitude, lng: position.coords.longitude}, function(data) {
if(data) {
$scope.searchInput = data;
$scope.dealerlocator.usingCurrentLocation = true;
$scope.search(e);
$scope.$apply();
}
});
},
function(error) {
var props = {
errorType: 'location unavailable',
language: GuxGlobals.siteSetup.OmnitureLanguage,
radius: $scope.filter.distance,
searchSelection: $scope.dealerlocator.currentSearchType,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
$scope.dealerlocator.errorMessage = $('.error-geolocation-unavailable').text();
$scope.$apply();
});
};
})
.controller('DealerResultsController', function($scope, $routeParams, $rootScope, $timeout, $location, $q) {
$scope.sizeElements();
$scope.params = $routeParams;
$scope.params.encodedSearchParams = encodeSlash($scope.params.searchParam);
$scope.dealerlocator.searchParams = $scope.params;
if(typeof GuxGlobals.currentSearchParams == 'undefined'){
GuxGlobals.currentSearchParams = $scope.params.searchParam;
$scope.dealerlocator.isSearchEnabled =true;
}
if(GuxGlobals.currentSearchParams != $scope.params.searchParam){
$scope.dealerlocator.isSearchEnabled =true;
if(typeof $scope.params.searchParam != 'undefined'){
GuxGlobals.currentSearchParams = $scope.params.searchParam;
} 
}
if(typeof $scope.dealerlocator.dealer == 'undefined'){
$scope.dealerlocator.dealer = null;
}
$scope.dealerlocator.displaySplash = false;
$scope.showDetails = function(index) {
$scope.dealerlocator.dealer = $scope.dealerlocator.dealers[index];
$scope.dealerlocator.dealer.index = index;
$scope.dealerlocator.dealer.distance = $scope.dealerlocator.dealers[index].distance;
$scope.dealerlocator.map.deselectMarkers();
if(!$scope.desktop.matches) {
$('body').scrollTop($('.dl-results').offset().top);
}
};
$scope.changePath = function(index) {
$location.path('search/'+encodeSlash($scope.params.searchParam)+'/dealer/'+$scope.dealerlocator.dealers[index].EntityID);
$scope.dealerlocator.resultsVisible = true;
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.dealerlocator.dealer = $scope.dealerlocator.dealers[index];
$scope.dealerlocator.dealer.index = index;
$scope.dealerlocator.dealer.distance = $scope.dealerlocator.dealers[index].distance;
$scope.dealerlocator.map.deselectMarkers();
if(!$scope.desktop.matches) {
$('body').scrollTop($('.dealer-details').offset().top);
}
};
$scope.showMoreDealers = function() {
var props;
$scope.page++;
if($scope.dealerlocator.filtersUpdated) {
props = {
deviceType : "mobile", 
pageNumber : $scope.page+1, 
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchType : $scope.dealerlocator.usingCurrentLocation ? 'current location' : 'manual search', 
searchSelection: $scope.dealerlocator.currentSearchType,
totalResults : $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1, 
radius : $scope.filter.distance,
dealerFilterType: 'services'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:advanced_search:results:page_<pageNumber>', props);
} else {
props = {
deviceType : "mobile", 
pageNumber : $scope.page+1, 
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchType : $scope.dealerlocator.usingCurrentLocation ? 'current location' : 'manual search', 
searchSelection: $scope.dealerlocator.currentSearchType,
totalResults : $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1, 
radius : $scope.filter.distance
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:results:page_<pageNumber>', props);
}
$scope.dealerlocator.dealers = $scope.dealerlocator.totalDealers.slice(0, ($scope.page + 1) * 5);
$scope.dealerlocator.map.displayDealers($scope.dealerlocator.dealers);
$scope.dealerlocator.map.setBounds($scope.dealerlocator.dealers);
$scope.dealerlocator.map.setZoom($scope.dealerlocator.map.getZoom() - 2);
};
$scope.search = function() {
$scope.sizeElements();
$scope.page = 0;
$scope.dealerlocator.currentSearchType = 'location';
var searchPromise = $q.defer();
searchPromise.promise.then(function() {
// Don't need to do anything if search succeeds
},
function() {
$scope.dealerlocator.errorMessage = $('.error-critical').text();
$scope.dealerlocator.searchLoading = false;
$scope.sizeElements();
var props = {
errorType: 'system error',
location: $scope.dealerlocator.currentSearchLocation.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
radius: $scope.filter.distance,
searchSelection: $scope.dealerlocator.currentSearchType,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
});
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.dealerlocator.resultsVisible = true;
$scope.dealerlocator.searchLoading = true;
$scope.dealerlocator.map.geocodeLocation($routeParams.searchParam, function(locations) {
var filters = {
CountryCode: GuxGlobals.isoCountry
};
for (var i = 0; i < $scope.config.filters.length; i++) {
if($scope.config.filters[i].selected) {
filters[$scope.config.filters[i].option] = 1;
}
}
if($scope.dealerlocator.disambiguationLatLng) {
locations = [$scope.dealerlocator.disambiguationLatLng];
}
if(locations.length === 1) {
$scope.dealerlocator.currentSearchLocation = $routeParams.searchParam.toLowerCase();
$timeout(function() {
searchPromise.reject();
}, 20000);
$scope.dealerlocator.map.searchDealersByDistance(locations[0], $scope.filter.distance * $scope.config.distances.distanceConversion, $scope.config.displayLimit, function(dealers) {
var props,
servicesUpdated = [],
i;
searchPromise.resolve();
function dealerCallback(dealer, index) {
return function() {
if($scope.dealerlocator.isDisplayDealerDetails){
$scope.dealerlocator.isPushPinClicked = true;
$scope.changePath(index);
}
$scope.dealerlocator.map.deselectMarkers();
$scope.dealerlocator.map.selectMarker(index);
$scope.dealerlocator.map.selectedMarker = $scope.dealerlocator.map.markers[index];
$scope.dealerlocator.dealer = $scope.dealerlocator.dealers[index];
$scope.dealerlocator.dealer.index = index;
$scope.dealerlocator.dealer.distance = $scope.dealerlocator.dealers[index].distance; 
$scope.selectResult(index);
if($('.result').eq(index).length > 0) {
if($scope.desktop.matches) {
$('.dl-results-list').scrollTop($('.result').eq(index).position().top);
} else {
$('body').scrollTop($('.result').eq(index).offset().top);
}
}
$scope.dealerlocator.map.panTo(dealer.location);
var props = {
location: $scope.dealerlocator.currentSearchLocation.toLowerCase(),
dealerCode: dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: index + 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:results:map_select', props);
};
}
for (i = 0; i < dealers.length; i++) {
dealers[i].directionsURL = $scope.dealerlocator.map.getAddressDirectionsURL(dealers[i], $scope.dealerlocator.addressFormat, locations[0]);
dealers[i].callback = dealerCallback(dealers[i], i);
// dealers[i].infoWindowMarkup = generateInfoWindow(dealers[i]);
processDealer(dealers[i], $scope.config.filters, i, dealers.length, $scope.dealerlocator);
}
$scope.dealerlocator.totalDealers = dealers;
$scope.dealerlocator.dealers = $scope.dealerlocator.totalDealers.slice(0, ($scope.page + 1) * 5);
$scope.$apply();
$scope.sizeElements();
$scope.dealerlocator.map.clearDealers();
$scope.dealerlocator.map.displayDealers($scope.dealerlocator.dealers);
if($scope.dealerlocator.totalDealers.length) {
$scope.dealerlocator.map.setBounds($scope.dealerlocator.dealers.concat(locations[0])); //, $scope.offset);
if($scope.dealerlocator.map.getZoom() > 14) {
$scope.dealerlocator.map.setZoom(14);
} else {
$scope.dealerlocator.map.setZoom($scope.dealerlocator.map.getZoom() - 2);
}
}
if($scope.dealerlocator.totalDealers.length === 0) {
if($scope.dealerlocator.filtersUpdated) {
props = {
deviceType : "mobile", 
location: $scope.dealerlocator.currentSearchLocation.toLowerCase(),
errorType: 'zero results',
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchSelection: $scope.dealerlocator.currentSearchType,
radius : $scope.filter.distance
};
TK.broadcast(document.body, 'brand:dealer_locater:advanced_search:<errorType>', props);
} else {
props = {
deviceType : "mobile", 
location: $scope.dealerlocator.currentSearchLocation.toLowerCase(),
radius : $scope.filter.distance,
errorType: 'zero results',
searchSelection: $scope.dealerlocator.currentSearchType,
language: GuxGlobals.siteSetup.OmnitureLanguage
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
}
$scope.dealerlocator.errorMessage = $('.error-no-results').text();
$scope.filterSettings.hidden = false;
$scope.dealerlocator.searchLoading = false; 
$scope.sizeElements();
$scope.$apply();
$scope.sizeElements();
} else {
for(i = 0; i < $scope.config.filters.length; i++) {
if($scope.config.filters[i].selected) {
servicesUpdated.push($scope.config.filters[i].option.toLowerCase());
}
}
if($scope.dealerlocator.filtersUpdated) {
props = {
deviceType : "mobile", 
pageNumber : $scope.page+1, 
location : $scope.dealerlocator.currentSearchLocation.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchType : $scope.dealerlocator.usingCurrentLocation ? 'current location' : 'manual search', 
searchSelection: $scope.dealerlocator.currentSearchType,
totalResults : $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1, 
radius : $scope.filter.distance,
dealerFilterType: 'services'
};
if(servicesUpdated.length > 0) {
props.facetSelection = servicesUpdated.join(',');
}
TK.broadcast(document.body, 'brand:dealer_locater:advanced_search:results:page_<pageNumber>', props);
} else {
props = {
deviceType : "mobile", 
pageNumber : $scope.page+1, 
location : $scope.dealerlocator.currentSearchLocation.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchSelection: 'location',
searchType : $scope.dealerlocator.usingCurrentLocation ? 'current location' : 'manual search',
totalResults : $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance
};
if(servicesUpdated.length > 0) {
props.facetSelection = servicesUpdated.join(',');
}
TK.broadcast(document.body, 'brand:dealer_locater:results:page_<pageNumber>', props);
}
$scope.filterSettings.hidden = true;
$scope.dealerlocator.errorMessage = null;
$scope.dealerlocator.searchLoading = false;
$scope.$apply();
$scope.sizeElements();
}
}, filters );
} else {
if(locations.length > 1) {
TK.broadcast(document.body, 'brand:dealer_locater:error:disambiguation', {
deviceType : "mobile", 
searchSelection: 'location',
location : $routeParams.searchParam.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
radius: $scope.filter.distance
});
/*
$scope.dealerlocator.suggestions = {
locations: []
};
for (i = 0; i < locations.length; i++) {
if($.inArray(locations[i].description, $scope.dealerlocator.suggestions.locations) === -1) {
$scope.dealerlocator.suggestions.locations.push(locations[i].description);
}
}
*/
$scope.dealerlocator.suggestions = {
disambiguations: []
};
for (i = 0; i < locations.length; i++) {
$scope.dealerlocator.suggestions.disambiguations.push(locations[i]);
}
$scope.dealerlocator.searchLoading = false;
$scope.$apply();
$scope.sizeElements();
} else {
$scope.dealerlocator.errorMessage = $('.error-no-locations').text();
$scope.dealerlocator.totalDealers = [];
$scope.dealerlocator.dealers = [];
$scope.dealerlocator.map.clearDealers();
// if no location results on 2nd search, routed back to main controller
if (!$scope.dealerlocator.filtersUpdated) {
// set to true to stop filter menu sliding back after no location search and then making a correct location search
$scope.filterSettings.hidden = true;
$scope.dealerlocator.dealer = undefined;
$scope.dealerlocator.dealers = undefined;
}
$scope.dealerlocator.searchLoading = false;
$scope.$apply();
$scope.sizeElements();
var props = {
errorType: 'invalid location',
location: $routeParams.searchParam.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
radius: $scope.filter.distance,
searchSelection: $scope.dealerlocator.currentSearchType,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
}
}
});
};
$scope.nameSearch = function() {
$scope.dealerlocator.currentSearchType = 'dealer name';
var searchPromise = $q.defer(),
props;
searchPromise.promise.then(function() {
// Don't need to do anything if search succeeds
},
function() {
$scope.dealerlocator.errorMessage = $('.error-critical').text();
$scope.dealerlocator.searchLoading = false;
$scope.sizeElements();
var props = {
errorType: 'system error',
language: GuxGlobals.siteSetup.OmnitureLanguage,
radius: $scope.filter.distance,
searchSelection: $scope.dealerlocator.currentSearchType,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
});
var filters = {
CountryCode: GuxGlobals.isoCountry
};
for (var i = 0; i < $scope.config.filters.length; i++) {
if($scope.config.filters[i].selected) {
filters[$scope.config.filters[i].option] = 1;
}
}
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.dealerlocator.resultsVisible = true;
$scope.dealerlocator.searchLoading = true;
//$routeParams.encodedSearchParams = encodeMapCharacters($routeParams.searchParam);
$timeout(function() {
searchPromise.reject();
}, 20000);
$scope.dealerlocator.map.searchDealersByProperties($scope.config.displayLimit, function(dealers) {
searchPromise.resolve();
// ENCODING CHARACTERS FOR MAPS API RENDERING
$scope.dealerlocator.currentSearchName = $routeParams.searchParam;
function dealerCallback(dealer, index) {
return function() {
if($scope.dealerlocator.isDisplayDealerDetails){
$scope.dealerlocator.isPushPinClicked = true;
$scope.changePath(index);
} else {
$scope.dealerlocator.resultsVisible = true;
$scope.$apply();
}
$scope.dealerlocator.map.deselectMarkers();
$scope.dealerlocator.map.selectMarker(index);
$scope.dealerlocator.map.selectedMarker= $scope.dealerlocator.map.markers[index];
$scope.dealerlocator.dealer = $scope.dealerlocator.dealers[index];
$scope.dealerlocator.dealer.index = index;
$scope.dealerlocator.dealer.distance = $scope.dealerlocator.dealers[index].distance; 
$scope.selectResult(index);
if($('.result').eq(index).length > 0) {
if($scope.desktop.matches) {
$('.dl-results-list').scrollTop($('.result').eq(index).position().top);
} else {
$('body').scrollTop($('.result').eq(index).offset().top);
}
}
$scope.dealerlocator.map.panTo(dealer.location);
var props = {
namesearchTerm: $scope.dealerlocator.currentSearchName.toLowerCase(),
dealerCode: dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: index + 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:results:map_select', props);
};
}
for (var i = 0; i < dealers.length; i++) {
dealers[i].directionsURL = $scope.dealerlocator.map.getAddressDirectionsURL(dealers[i], $scope.dealerlocator.addressFormat);
dealers[i].callback = dealerCallback(dealers[i], i);
// dealers[i].infoWindowMarkup = generateInfoWindow(dealers[i]);
processDealer(dealers[i], $scope.config.filters, i, dealers.length, $scope.dealerlocator);
}
$scope.dealerlocator.totalDealers = dealers;
$scope.dealerlocator.dealers = $scope.dealerlocator.totalDealers.slice(0, ($scope.page + 1) * 5);
$scope.dealerlocator.searchLoading = false;
$scope.$apply();
$scope.sizeElements();
$scope.dealerlocator.map.clearDealers();
$scope.dealerlocator.map.displayDealers($scope.dealerlocator.dealers);
if($scope.dealerlocator.totalDealers.length) {
$scope.dealerlocator.map.setBounds($scope.dealerlocator.dealers);
if($scope.dealerlocator.map.getZoom() > 14) {
$scope.dealerlocator.map.setZoom(14);
} else {
$scope.dealerlocator.map.setZoom($scope.dealerlocator.map.getZoom() - 2);
}
}
if($scope.dealerlocator.totalDealers.length === 0) {
if($scope.dealerlocator.filtersUpdated) {
props = {
deviceType : "mobile", 
errorType: 'zero results',
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchSelection: $scope.dealerlocator.currentSearchType,
radius : $scope.filter.distance,
namesearchTerm: $scope.dealerlocator.currentSearchName.toLowerCase()
};
TK.broadcast(document.body, 'brand:dealer_locater:advanced_search:<errorType>', props);
} else {
props = {
deviceType : "mobile",
totalResults: 0,
pageNumber : 1, 
radius : $scope.filter.distance,
searchSelection: $scope.dealerlocator.currentSearchType,
errorType: 'zero results',
language: GuxGlobals.siteSetup.OmnitureLanguage,
namesearchTerm: $scope.dealerlocator.currentSearchName.toLowerCase()
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
}
$scope.dealerlocator.errorMessage = $('.error-no-results').text();
// if no results on 2nd search, routed back to main controller
if (!$scope.dealerlocator.filtersUpdated) {
// set to true to stop filter menu sliding back after no dealer search and then making a correct dealer search
$scope.filterSettings.hidden = true;
$scope.dealerlocator.dealer = undefined;
$scope.dealerlocator.dealers = undefined;
//$location.path('/');
} else {
// set to false to allow filter menu to remain open when filtered selected options produce no dealerships
$scope.filterSettings.hidden = false;
}
$scope.$apply();
$scope.sizeElements();
} else {
$scope.filterSettings.hidden = true;
$scope.dealerlocator.errorMessage = null;
$scope.$apply();
$scope.sizeElements();
if($scope.dealerlocator.filtersUpdated) {
props = {
deviceType : "mobile", 
pageNumber : $scope.page+1, 
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchType : $scope.dealerlocator.usingCurrentLocation ? 'current location' : 'manual search', 
searchSelection: $scope.dealerlocator.currentSearchType,
totalResults : $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1, 
radius : $scope.filter.distance,
dealerFilterType: 'services',
namesearchTerm: $scope.dealerlocator.currentSearchName.toLowerCase()
};
TK.broadcast(document.body, 'brand:dealer_locater:advanced_search:results:page_<pageNumber>', props);
} else {
props = {
deviceType : "mobile", 
pageNumber : $scope.page+1, 
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchSelection: 'dealer name',
searchType : 'manual search', 
totalResults : $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
namesearchTerm: $scope.dealerlocator.currentSearchName.toLowerCase()
};
TK.broadcast(document.body, 'brand:dealer_locater:results:page_<pageNumber>', props);
}
}
}, filters, {DealerNameSearch:encodeMapCharacters($routeParams.searchParam).toLowerCase()});
};
$scope.$on('filters-updated', function(e) {
var servicesUpdated = [],
separator = '',
props;
for(var i = 0; i < $scope.config.filters.length; i++) {
if($scope.config.filters[i].selected) {
servicesUpdated.push($scope.config.filters[i].option.toLowerCase());
}
}
props = {
deviceType : "mobile",  
radius : $scope.filter.distance,
language: GuxGlobals.siteSetup.OmnitureLanguage,
dealerFilterType: 'services',
facetSelection: servicesUpdated.join(',')
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:results:advanced_search:submit', props);
$scope.dealerlocator.filtersUpdated = true;
if($scope.dealerlocator.searchType === 'location') {
$scope.search();
} else {
if($scope.dealerlocator.searchType === 'dealername') {
$scope.nameSearch();
}
}
$scope.dealerlocator.isSearchEnabled = false;
if(!$scope.desktop.matches) {
$('body').scrollTop($('.dl-search-type').offset().top);
}
//$scope.filterSettings.hidden = true;
});
$scope.selectResult = function(index) {
for (var i = 0; i < $scope.dealerlocator.dealers.length; i++) {
$scope.dealerlocator.dealers[i].selected = false;
}
$scope.dealerlocator.dealers[index].selected = true;
$scope.$apply();
};
$scope.showResultsBlock = function() {
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.dealerlocator.resultsVisible = true;
if(typeof $scope.dealerlocator.map.selectedMarker != "undefined"){
$scope.dealerlocator.map.selectedMarker.hideInfowindow();
}
};
if($scope.dealerlocator.searchType === 'location') {
if((typeof $scope.dealerlocator.dealers == 'undefined' || $scope.dealerlocator.dealers.length<=0)||(typeof $scope.dealerlocator.isSearchEnabled == 'undefined')||(typeof $scope.dealerlocator.isSearchEnabled != 'undefined' && $scope.dealerlocator.isSearchEnabled)){
$scope.search();
$scope.dealerlocator.isSearchEnabled=false;
} else {
$scope.$on('$viewContentLoaded', function(){
$timeout(function() {
$scope.selectResult($scope.dealerlocator.dealer.index);
if($scope.desktop.matches) {
$('.dl-results-list').scrollTop($('.result').eq($scope.dealerlocator.dealer.index).position().top);
} else {
$('body').scrollTop($('.result').eq($scope.dealerlocator.dealer.index).offset().top);
}
},0);
});
}
} else {
if($scope.dealerlocator.searchType === 'dealername') {
if((typeof $scope.dealerlocator.dealers == 'undefined' || $scope.dealerlocator.dealers.length<=0)||(typeof $scope.dealerlocator.isSearchEnabled == 'undefined')||(typeof $scope.dealerlocator.isSearchEnabled != 'undefined' && $scope.dealerlocator.isSearchEnabled)){
$scope.nameSearch();
$scope.dealerlocator.isSearchEnabled=false;
} else {
$scope.$on('$viewContentLoaded', function(){
$timeout(function() {
$scope.selectResult($scope.dealerlocator.dealer.index);
if($scope.desktop.matches) {
$('.dl-results-list').scrollTop($('.result').eq($scope.dealerlocator.dealer.index).position().top);
} else {
$('body').scrollTop($('.result').eq($scope.dealerlocator.dealer.index).offset().top);
}
},0);
});
}
}
}
$scope.$watch(
function($scope) { 
return $scope.dealerlocator.fromFilter;
},
function() {
if((typeof $scope.dealerlocator.fromFilter != 'undefined' && $scope.dealerlocator.fromFilter) ) {
window.scrollTo($('#search-field').offset().top,$('#search-field').offset().left);
$('.filters').scrollTop(0);
$scope.dealerlocator.fromFilter =false;
}
}
);
$scope.dealerlocator.isDisplayDealerDetails = false;
// $scope.nameSearch();
})
.controller('DealerDetailsController', function($scope, $timeout, $q, $routeParams, $location) {
$scope.params = $routeParams;
$scope.params.encodedSearchParams = encodeSlash($scope.params.searchParam);
$scope.filterSettings.hidden = true;
$scope.dealerlocator.displaySplash = false;
$scope.dealerlocator.searchParams = $scope.params;
if(typeof GuxGlobals.currentSearchParams == 'undefined'){
GuxGlobals.currentSearchParams = $scope.params.searchParam;
$scope.dealerlocator.isSearchEnabled = true;
}
if(GuxGlobals.currentSearchParams != $scope.params.searchParam){
if(typeof $scope.params.searchParam != 'undefined'){
GuxGlobals.currentSearchParams = $scope.params.searchParam;
} 
$scope.dealerlocator.isSearchEnabled = true;
}else{
}
$scope.sizeElements();
$scope.$on('$viewContentLoaded', function(){
$timeout(function() {
if(typeof $scope.dealerlocator.isPushPinClicked != 'undefined' && $scope.dealerlocator.isPushPinClicked){
$scope.dealerlocator.isPushPinClicked=false;
}else{				
$scope.showResultsBlock();
}
},0);
});
$scope.showResultsBlock = function() {
$scope.dealerlocator.resultsAlwaysVisible = true;
$scope.dealerlocator.resultsVisible = true;
if(typeof $scope.dealerlocator.map.selectedMarker != 'undefined'){
$scope.dealerlocator.map.selectedMarker.hideInfowindow();
}
};
$scope.dealerlocator.isDisplayDealerDetails = true;
var searchPromise = $q.defer();
searchPromise.promise.then(function() {
// Don't need to do anything if search succeeds
},
function() {
$scope.dealerlocator.errorMessage = $('.error-critical').text();
$scope.dealerlocator.searchLoading = false;
$scope.sizeElements();
var props = {
errorType: 'system error',
location: $scope.dealerlocator.currentSearchLocation.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
searchSelection: $scope.dealerlocator.currentSearchType,
radius: $scope.filter.distance,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
});
var filters = {
CountryCode: GuxGlobals.isoCountry,
EntityID: $scope.params.dealerID
};
if(!$scope.dealerlocator.isSearchEnabled && $scope.dealerlocator.dealer && $scope.dealerlocator.dealer.EntityID === $scope.params.dealerID) {
$scope.dealerlocator.map.selectMarker($scope.dealerlocator.dealer.index);
$scope.dealerlocator.map.selectedMarker= $scope.dealerlocator.map.markers[$scope.dealerlocator.dealer.index];
$scope.dealerlocator.map.setCenter($scope.dealerlocator.dealer.location); //, $scope.offset);
var props = {
dealerCode: $scope.dealerlocator.dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: $scope.dealerlocator.dealer.index + 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:dealer:info', props);
} else {
$scope.dealerlocator.isSearchEnabled =false;
if($scope.params.searchParam) {
$scope.dealerlocator.searchLoading = true;
$timeout(function() {
searchPromise.reject();
}, 20000);
$scope.dealerlocator.map.geocodeLocation($scope.params.searchParam, function(locations) {
if(locations.length === 1) {
$scope.dealerlocator.map.searchDealersByDistance(locations[0], null, $scope.config.displayLimit, function(dealers) {
if(dealers && dealers.length >=1) {
searchPromise.resolve();
$scope.dealerlocator.dealer = dealers[0];
$scope.dealerlocator.dealer.directionsURL = $scope.dealerlocator.map.getAddressDirectionsURL($scope.dealerlocator.dealer, $scope.dealerlocator.addressFormat);
$scope.dealerlocator.dealer.callback = function(dealer, index) {
return function() {
//$scope.dealerlocator.resultsVisible = true;
//$scope.$apply();
$scope.dealerlocator.map.deselectMarkers();
$scope.dealerlocator.map.selectMarker(index);
$scope.dealerlocator.map.selectedMarker= $scope.dealerlocator.map.markers[index];
$scope.dealerlocator.dealer = $scope.dealerlocator.dealers[index];
$scope.dealerlocator.dealer.index = index;
$scope.dealerlocator.dealer.distance = $scope.dealerlocator.dealers[index].distance;
var props = {
dealerCode: dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: index + 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:results:map_select', props);
};
}(dealers[0], 0);
processDealer($scope.dealerlocator.dealer, $scope.config.filters, 0, 1, $scope.dealerlocator);
$scope.dealerlocator.searchLoading = false;
$scope.dealerlocator.map.displayDealers([$scope.dealerlocator.dealer]);
$scope.dealerlocator.map.setCenter($scope.dealerlocator.dealer.location); //, $scope.offset);
$scope.dealerlocator.map.setZoom(14);
$scope.$apply();
$scope.sizeElements();
var props = {
dealerCode: $scope.dealerlocator.dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: $scope.dealerlocator.dealer.index ? $scope.dealerlocator.dealer.index + 1 : 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:dealer:info', props);
} else {
searchPromise.reject();
}
}, filters);
} else {
searchPromise.resolve();
$location.path('/search/' + $scope.params.searchParam);
}
});
} else {
$scope.dealerlocator.searchLoading = true;
$timeout(function() {
searchPromise.reject();
}, 20000);
$scope.dealerlocator.map.searchDealersByProperties(1, function(dealers) {
if(dealers && dealers.length > 0) {
searchPromise.resolve();
$scope.dealerlocator.dealer = dealers[0];
$scope.dealerlocator.dealer.directionsURL = $scope.dealerlocator.map.getAddressDirectionsURL($scope.dealerlocator.dealer, $scope.dealerlocator.addressFormat);
$scope.dealerlocator.dealer.callback = function(dealer, index) {
return function() {
$scope.dealerlocator.map.deselectMarkers();
$scope.dealerlocator.map.selectMarker(index);
$scope.dealerlocator.map.selectedMarker= $scope.dealerlocator.map.markers[index];
var props = {
namesearchTerm: $scope.dealerlocator.dealer.DealerName.toLowerCase(),
dealerCode: dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: index + 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:results:map_select', props);
};
}(dealers[0], 0);
processDealer($scope.dealerlocator.dealer, $scope.config.filters, 0, 1, $scope.dealerlocator);
$scope.dealerlocator.searchLoading = false;
$scope.dealerlocator.map.displayDealers([$scope.dealerlocator.dealer]);
$scope.dealerlocator.map.setCenter($scope.dealerlocator.dealer.location); //, $scope.offset);
$scope.dealerlocator.map.setZoom(14);
$scope.updateSearchField($scope.dealerlocator.dealer.DealerName);
$scope.$apply();
$scope.sizeElements();
var props = {
dealerCode: $scope.dealerlocator.dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: $scope.dealerlocator.dealer.index ? $scope.dealerlocator.dealer.index + 1 : 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
deviceType: 'mobile'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:dealer:info', props);
} else {
searchPromise.reject();
}
}, filters);
}
}
$scope.openServices = function(type, e) {
// currently used for analytics
if(!$(e.target).parent().hasClass('accordian-open')) {
var props = {
dealerCode: $scope.dealerlocator.dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: $scope.dealerlocator.dealer.index + 1 || 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
infoType: type,
deviceType: 'mobile',
actionType: 'expand'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:dealer:info:cta', props);
TK.broadcast(document.body, 'brand:dealer_locater:dealer:info:<infoType>', props);
}
};
$scope.dealerCTA = function(e) {
var ctaName = $(e.target).eq(0).text().toLowerCase(),
props = {
dealerCode: $scope.dealerlocator.dealer.EntityID.toLowerCase(),
language: GuxGlobals.siteSetup.OmnitureLanguage,
position: $scope.dealerlocator.dealer.index + 1 || 1,
totalResults: $scope.dealerlocator.totalDealers ? $scope.dealerlocator.totalDealers.length : 1,
radius : $scope.filter.distance,
infoType: ctaName,
deviceType: 'mobile',
actionType: 'select'
};
if($scope.dealerlocator.currentSearchType === 'location' && $scope.dealerlocator.currentSearchLocation) {
props.location = $scope.dealerlocator.currentSearchLocation.toLowerCase();
}
if($scope.dealerlocator.currentSearchType === 'dealer name' && $scope.dealerlocator.currentSearchName) {
props.namesearchTerm = $scope.dealerlocator.currentSearchName.toLowerCase();
}
TK.broadcast(document.body, 'brand:dealer_locater:dealer:info:cta', props);
};
$scope.$on('filters-updated', function(e) {
if($scope.params.searchParam) {
$location.path('/search/' + $scope.params.searchParam);
} else {
$location.path('/search/' + $scope.dealerlocator.dealer.DealerName);
}
});
});
function formatTime(time) {
var hours, 
minutes;
if(time.length < 4) {
time = 0 + time;
}
hours = time.substr(0,2);
time = hours + GuxGlobals.timeDelimiter1+ time.substr(2,2);
return time;
}
function processDealer(dealer, filters, index, totalResults, dealerlocator) {
var daysDisplayOrder = ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday'], // Display order - should probably come from service
jsDaysOrder = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'], // Javascript Date order
today = new Date(),
services = [],
i;
for(i = 0; i < daysDisplayOrder.length; i++) {
if(dealer['Sales' + daysDisplayOrder[i] + 'OpenTime'] && dealer['Sales' + daysDisplayOrder[i] + 'CloseTime']) {
dealer['Sales' + daysDisplayOrder[i] + 'OpenTime'] = formatTime(dealer['Sales' + daysDisplayOrder[i] + 'OpenTime']);
dealer['Sales' + daysDisplayOrder[i] + 'CloseTime'] = formatTime(dealer['Sales' + daysDisplayOrder[i] + 'CloseTime']);
}
}
if(dealer['Sales' + jsDaysOrder[today.getDay()] + 'CloseTime']) {
dealer.todayClosingTime = dealer['Sales' + jsDaysOrder[today.getDay()] + 'CloseTime'];
}
if(dealer.PrimaryPhone) {
dealer.PrimaryPhoneURL = dealer.PrimaryPhone.replace(/\s/g, '');
}
if(dealer.PrimaryURL) {
dealer.PrimaryURL = dealer.PrimaryURL.substr(0, 7) === 'http://' ? dealer.PrimaryURL : 'http://' + dealer.PrimaryURL;
}
if(dealer.ServiceURL) {
dealer.ServiceURL = dealer.ServiceURL.substr(0, 7) === 'http://' ? dealer.ServiceURL : 'http://' + dealer.ServiceURL;
}
if(filters) {
for(i = 0; i < filters.length; i++) {
if(dealer[filters[i].option] == 1) {
services.push(filters[i].display);
}
}
if(services.length) {
dealer.serviceList = services.join(', ');
}
}
dealer.infoWindowMarkup = generateInfoWindow(dealer, index, totalResults, dealerlocator);
}
function generateInfoWindow(dealer, index, totalResults, dealerlocator) {
var infowindow,
dealerName,
hours,
address,
phone,
primarySite,
serviceSite,
directions,
fullDealerDetails,
openUntilText,
directionsLinkText,
dealerWebsite,
serviceWebsite,
servicesText,
searchType = '';
dealerlocator.searchParams.searchParam = encodeSlash(dealerlocator.searchParams.searchParam);
if(dealerlocator.currentSearchType === 'location' && dealerlocator.currentSearchLocation) {
searchType = 'location: \'' + dealerlocator.currentSearchLocation + '\',';
}
if(dealerlocator.currentSearchType === 'dealer name' && dealerlocator.currentSearchName) {
searchType = 'namesearchTerm: \'' + dealerlocator.currentSearchName.toLowerCase() + '\',';
}
openUntilText = $($('#details-template').html()).find('.dl-hours').text().replace(/{{.+}}/, '');
directionsLinkText = $($('#details-template').html()).find('.dl-directions a').text();
dealerWebsite = $($('#details-template').html()).find('.dealerWebsiteLink');
serviceWebsite = $($('#details-template').html()).find('.serviceWebsiteLink');
if(typeof dealerlocator.searchParams.searchParam !='undefined' ){
dealerName = dealer.DealerName ? '<h5 class="dl-dealer-name"><a onclick="$(\'#dealer-locator\').scope().showResultsBlock()"  data-ng-href="#/search/'+dealerlocator.searchParams.searchParam+'/dealer/'+ dealer.EntityID+ '/"  href="#/search/'+dealerlocator.searchParams.searchParam+'/dealer/'+ dealer.EntityID+ '/" class="ng-binding" >' + dealer.DealerName + '</a></h5><a onclick="$(\'#dealer-locator\').scope().dealerlocator.map.selectedMarker.hideInfowindow()" class="close-info-window"></a>' : '';
} else {
dealerName = dealer.DealerName ? '<h5 class="dl-dealer-name"><a onclick="$(\'#dealer-locator\').scope().showResultsBlock()"  data-ng-href="#/dealer/'+ dealer.EntityID+ '/" href="#/dealer/'+ dealer.EntityID+ '/" class="ng-binding" >' + dealer.DealerName + '</a></h5><a onclick="$(\'#dealer-locator\').scope().dealerlocator.map.selectedMarker.hideInfowindow()" class="close-info-window"></a>' : '';
}
hours = dealer.todayClosingTime ? '<p class="dl-closing-time">' + openUntilText + dealer.todayClosingTime + '</p>' : '';
servicesText = dealer.serviceList ? '<p class="dl-services">' + dealer.serviceList + '</p>' : '';
address = '';
for (var i = 0; i < addressFormat.length; i++) {
if(dealer[addressFormat[i]]) {
address += '<p>' + dealer[addressFormat[i]] + '</p>';
}
}
var websiteTracking = 'TK.broadcast(document.body, \'brand:dealer_locator:referral:weblink\', {' + searchType + 'dealerCode: \'' + dealer.EntityID.toLowerCase() + '\', language: \'' + GuxGlobals.siteSetup.OmnitureLanguage + '\', position: \'' + (index+1) + '\', totalResults: \'' + totalResults + '\', radius: ' + 20 + ',deviceType: \'mobile\', ctaPlacement: \'map view\'})';
var directionsTracking = 'TK.broadcast(document.body, \'brand:dealer_locator:get_directions\', {' + searchType + 'dealerCode: \'' + dealer.EntityID.toLowerCase() + '\', language: \'' + GuxGlobals.siteSetup.OmnitureLanguage + '\', position: \'' + (index+1) + '\', totalResults: \'' + totalResults + '\', radius: ' + 20 + ',deviceType: \'mobile\', ctaPlacement: \'map view\'})';
phone = dealer.PrimaryPhone ? '<p class="dl-telephone"><span class="icon-phone"></span>' + dealer.PrimaryPhone + '</p>' : '';
primarySite = dealer.PrimaryURL ? '<p><a class="ext-link" onclick="' + websiteTracking + '" href="' + dealer.PrimaryURL +'" target="_blank" title="' + dealerWebsite.attr('title') + '">' + dealerWebsite.html() + '</a></p>': '';
serviceSite = dealer.ServiceURL ? '<p><a class="ext-link" onclick="' + websiteTracking + '" href="' + dealer.ServiceURL +'" target="_blank" title="' + serviceWebsite.attr('title') + '">' + serviceWebsite.html() + '</a></p>': '';
if(typeof dealerlocator.searchParams.searchParam !='undefined' ) {
fullDealerDetails = '<p class="dl-infoBox-fullDetails"><a  class="dl-details" onclick="$(\'#dealer-locator\').scope().showResultsBlock()"  class="ng-binding" data-ng-href="#/search/'+dealerlocator.searchParams.searchParam+'/dealer/'+ dealer.EntityID+ '/" href="#/search/'+dealerlocator.searchParams.searchParam+'/dealer/'+ dealer.EntityID+ '/"><span class="icon-details dl-infoBox-span"></span>'+GuxGlobals.fullDetailsTxt+'<span class="offscreen">'+dealer.DealerName+'</span></a></p>';
} else {
fullDealerDetails ='<p class="dl-infoBox-fullDetails"><a  class="dl-details" onclick="$(\'#dealer-locator\').scope().showResultsBlock()"  class="ng-binding" data-ng-href="#/dealer/'+ dealer.EntityID+ '/" href="#/dealer/'+ dealer.EntityID+ '/"><span class="icon-details dl-infoBox-span"></span>'+GuxGlobals.fullDetailsTxt+'<span class="offscreen">'+dealer.DealerName+'</span></a></p>';
}
directions = dealer.directionsURL ? '<p class="dl-directions"><a onclick="' + directionsTracking + '" href="' + dealer.directionsURL + '" target="_blank"><span class="icon-directions"></span>' + directionsLinkText + '</a></p>' : '';
infowindow = dealerName + servicesText + hours + address + phone + primarySite + serviceSite + fullDealerDetails + directions;
return infowindow;
}
function checkMustang() {
var s = document.referrer.toLowerCase();
return (s.indexOf("mustang") > -1 || s.indexOf("n=00S") > -1);
}
// angular cannot use '/' in routing, have to encode it for it to work
function encodeSlash(uri) {
// when search params is undefined do not encode
if(typeof uri !== 'undefined'){
return uri.replace(/\//g, '%252F').replace(/%2F/gi, '%252F');
} else {
return uri;
}
}
function decodeSlash(uri) {
return uri.replace(/%2F/gi, "/");
}
function encodeMapCharacters(uri) {
/*
uri.replace(/\//g, '%252F').replace(/%2F/gi, '%252F'); // for /
uri.replace(/\&/g, '%2526').replace(/%26/gi, '%2526'); // for &
uri.replace(/\'/g, '%2527').replace(/%27/gi, '%2527'); // for '
*/
var url;
// REPLACING " & " AND " ' " WITH URL ENCODING 
url = uri.replace(/\&/g, '%26').replace(/\'/g, '%5C%27');
return url;
}
return module;
});
define('dealerlocator/directives/module',['angular', 'angular-sanitize', 'common'], function (angular, ngSanitize, common) {
var module = angular.module('dealerLocator.directives', ['ngSanitize']);
module.directive('guxDealerFilter', function($sanitize) {
return {
restrict: 'EA',
template: '<div class="columns small-8">' + 
'<p><input type="checkbox" ng-model="filter.selected" value="{{ filter.option }}" id="{{ filter.option }}" ng-class="{checked:filter.selected}" class="custom-checkbox">' + 
'<label for="{{ filter.option }}" ng-bind-html="filter.displayFilter"></label></p>' + 
'</div><div class="columns small-4">' +
'<button class="info-tooltip" ng-show="filter.info" ng-click="showFilterTooltip(filter)"><span class="icon-info"></span></button>' +
'</div>' + 
'<div class="columns small-11 large-10 tooltip-content-wrap" ng-class="{ hide: !filter.showTooltip }"><div class="tooltip-content">' + 
'<button class="tooltip-close" ng-click="filter.showTooltip = false"></button>' +
'<h6>{{ filter.displayFilter }}</h6><p>{{ filter.info }}</p></div></div>',
link: function (scope, element, attrs) {
}
};
});
return module;
});
define('dealerlocator/dealer-locator',['jquery', 'common', 'angular', 'angular-route', 'angular-sanitize', 'dealerlocator/controllers/module', 'dealerlocator/directives/module', 'header/globile.header'], function($, common, angular, ngRoute, ngSanitize, controllers, directives, globileHeader) {
// console.log('dealer locator module loaded');
function init() {
common.checkboxes('input[type=checkbox]');
common.accordians('#global-ux');
if($('#globile_header').length) {
globileHeader.init();
}
var app;
// console.log('dealer locator module initialised');
var script = document.createElement('script');
script.setAttribute('type', 'text/javascript');
script.setAttribute('src', 'https://maps.googleapis.com/maps/api/js?client=gme-fordmotorcompany2&libraries=places&channel=' + GuxGlobals.siteSetup.MarketCode + '&callback=initMap');
document.body.appendChild(script);
var serviceURL = window.location.hostname === 'localhost' ? '/assets/js/dealerlocator/config3.json' : '/cs/ContentServer?pagename=' + GuxGlobals.site + '/GlobalUX/DealerLocatorService',
dlService = $.getJSON(serviceURL); // This is done because we need to have the dealer locator config service loaded ASAP so that things like the country polygon are ready for the LAD API to be initialised.
window.initMap = function() {
if($('html').hasClass('lte9')) {
var props = {
errorType: 'unsupported browser',
language: GuxGlobals.siteSetup.OmnitureLanguage,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater:error:<errorType>', props);
} else {
dlService.done(function(response) {
GuxGlobals.dlService = response;
app = angular.module('dealerLocator',['dealerLocator.controllers', 'dealerLocator.directives']);
angular.bootstrap(document.getElementById('dealer-locator'), ['dealerLocator']);
});
}
};
var props = {
language: GuxGlobals.siteSetup.OmnitureLanguage,
deviceType: 'mobile'
};
TK.broadcast(document.body, 'brand:dealer_locater', props);
}
return {
init: init
};
});
requirejs.config({
baseUrl: '/assets/js',
paths: {
'noext': 'vendor/noext',
'jquery': 'vendor/jquery.min',
'jquery-cookie': 'vendor/jquery.cookie.min',
'respond': 'vendor/respond.min',
'matchMediaPolyfill': 'vendor/matchmedia.polyfill',
'matchMediaAddListener': 'vendor/matchmedia.addListener',
'angular': 'vendor/angular.min',
'async': 'vendor/async.min',
'angular-route': 'vendor/angular-route.min',
'angular-sanitize': 'vendor/angular-sanitize.min',
'hammer': 'vendor/hammer.min',
'handlebars': 'vendor/handlebars.min'
},
waitSeconds: 60,
shim: {
'angular': {
exports: 'angular'
},
'angular-route': {
deps: ['angular']
},
'angular-sanitize': {
deps: ['angular']
},
'respond': {
deps: ['matchMediaPolyfill', 'matchMediaAddListener']
},
'matchMediaAddListener': {
deps: ['matchMediaPolyfill']
}
}
});
require(['jquery', 'page-module/homepage-module', 'dealerlocator/dealer-locator'], function($, homepageModule, dl) {
$(document).ready(function() {
var pageName = $('#global-ux').data('page');
switch(pageName) {
case 'home':
homepageModule.init();
break;
case 'dealer_locater':
dl.init();
break;
default:
homepageModule.init();
break;
}
});
});
define("main", function(){});