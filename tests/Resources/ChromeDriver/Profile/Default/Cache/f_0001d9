define("handlebars", function(){});
define('personalisation/global.ux.personalise',['jquery', 'jquery-cookie', 'cookie/global.ux.cookie', 'handlebars'],
function($, jqueryCookie, guxCookie, handlebars) {
var FPSscript;
var props = {
'cookieData' : {},
'userData' : {},
'vehicleData' : {},
'fpsStatus' : {
'lastViewedVehicles' : false,
'viewedVehicles' : false,
'Visited' : false,
'UserDefinedLocation' : false,
'PreferredDealer' : false,
'ExternalRef' : false
},
'fpsReturnedData' : {},
'personalisedState' : false,
'omnitureValues' : {
'pageName' : '',
'load' : {
'deviceType' : 'mobile',
'cmTemplate' : 'new',
'cmPage' : 'hp'
}
}
};
/*
var getGeoData = function(){
//console.log('FUNCTION:getGeoData');
props.userData.location = PERSONALIZATION.getGeoData();
};
*/
var checkVehiclePageData = function(){
/*console.log('FUNCTION:checkVehiclePageData');*/
if(typeof(vehicleData) !== 'undefined'){
props.vehicleData.status = true;
/*console.log('VEHICLE PAGE DATA DOES EXIST!');*/
return true;
} else {
props.vehicleData.status = false;
/*console.log('VEHICLE PAGE DATA DOESN\'T EXIST!');*/
return false;
}
};
var setVehiclePageData = function(){
/*console.log('FUNCTION:setVehiclePageData');*/
if (checkVehiclePageData()){
props.vehicleData = vehicleData;
console.log('props.vehicleData = ');
console.log(props.vehicleData);
FPS.set([
{
'ViewedVehicle': {
_name: props.vehicleData.name,
_url: props.vehicleData.url
},
'Visited' : {
_url: props.vehicleData.url
}
}
/*
{
'ViewedVehicle': {
name: props.vehicleData.name,
url: props.vehicleData.url
},
'Visited' : {
url: props.vehicleData.url
},
'UserDefinedLocation': {
description: props.userData.location.City+','+props.userData.location.Country,
latitude : props.userData.location.Latitude,
longitude : props.userData.location.Longitude,
countryCode: props.userData.location.Longitude
},
'ExternalRef' : {
type: props.cookieData.type,
id: props.cookieData.id
}
}
*/
],
{
success: function (data, statusText, jqXHR) {
/*
console.log('FPS-DATA SET - SUCCESS!!');
console.log('set - success!');
console.log('data = ');
console.log(data);
*/
},
error: function (jqXHR, statusText, error) {
//console.log('FPS-DATA SET - FAILED!');
console.log('set - fail!');
}
}
);
}
};
var getPersonalisedData = function(){
/*console.log('FUNCTION:getPersonalisedData');*/
var FPSdeferredData = new $.Deferred();
FPS.getAll(
{
success: function(response, jqXHR, textStatus, errorThrown) {
/*
console.log('FPS.get SUCCESS = ');
console.log(response);
console.log(jqXHR);
console.log(textStatus);
console.log(errorThrown);
*/
if (response.length > 0){
props.personalisedState = true;
/*
console.log('PERSONALIZATION FULL RESPONSE = ');
console.log(response);
*/
for (var i = 0; i < response.length; i++){
for (var key in response[i]) {
props.fpsReturnedData[key] = response[i][key];
switch(key) {
case 'LastViewedVehicle':
if (props.fpsReturnedData[key].length > 0) {
props.fpsStatus.lastViewedVehicles = true;
}
break;
case 'Visited':
if (props.fpsReturnedData[key].length > 0) {
props.fpsStatus.Visited = true;
}
break;
case 'UserDefinedLocation':
if (props.fpsReturnedData[key].length > 0) {
props.fpsStatus.UserDefinedLocation = true;
}
break;
case 'ExternalRef':
props.fpsStatus.ExternalRef = true;
break;
case 'RecentlyViewedVehicles':
if (props.fpsReturnedData[key].length > 0) {
props.fpsStatus.viewedVehicles = true;
}
//console.log('props.fpsStatus.viewedVehicles = ' + props.fpsStatus.viewedVehicles);
break;
default:
//default code block
}
}
}
FPSdeferredData.resolve();
} else {
/*console.log('No returned data from FPS!');*/
FPSdeferredData.reject();
props.personalisedState = false;
}
},
error: function(response, jqXHR, textStatus, errorThrown) {
/*console.log('FPS.get FAIL = ');
console.log(response);
console.log(jqXHR);
console.log(textStatus);
console.log(errorThrown);*/
FPSdeferredData.reject();
}
});
return FPSdeferredData.promise();
};
var personaliseSetup = function(){
//console.log('FUNCTION:personaliseSetup');
props.cookieData = guxCookie.cookieInfo();
var cookieStatus = props.cookieData.status;
setVehiclePageData();
};
return {
init: function() {
//console.log('GUX PERSONALISE MODULE LOADED!!');
props.omnitureValues.load.cmTemplate = 'new';
props.omnitureValues.pageName = $('#global-ux').data('page');
if (!FPSscript){
FPSscript = new $.Deferred();
var url,
locale,
personalisationState,
personalisationUrl,
FPS_failTimer,
hostname = window.location.hostname;
if (typeof(GuxGlobals) !== 'undefined') {
locale = GuxGlobals.isoCountry;
personalisationState = GuxGlobals.personalisationState;
personalisationUrl = GuxGlobals.personalisationUrl+'/script/ford/'+locale;
FPS_failTimer = GuxGlobals.personalisationTimeout * 1000;
} else {
locale = 'GBR';
personalisationState = true;
personalisationUrl = 'http://eu.fpsqa.ford.com/script/ford/'+locale;
FPS_failTimer = 6 * 1000;
}
// personalisation site term switch
if (personalisationState) {
var FPS_failCheck =  setTimeout(function(){
//console.log('FPS ERROR!');
FPSscript.reject();
}, FPS_failTimer);
$.getScript(personalisationUrl)
.done(function( script, textStatus, jqxhr ) {
//console.log('FPS SCRIPT LOADED! - SUCCESS!');
clearTimeout(FPS_failCheck);
personaliseSetup();
getPersonalisedData().done(function(){
// if check required here if user is returning etc
if (props.fpsStatus.viewedVehicles){
props.omnitureValues.load.cmTemplate = 'return';
//omniture
if (GuxGlobals) {
GuxGlobals.omniturePersonalisedState = 'return';
}
} else {
GuxGlobals.omniturePersonalisedState = 'new';
}
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName, props.omnitureValues.load);
/* THE PROPS OBJECT IS PASSED THROUGH THE RESOLVE PROMISE FUNCTION */
FPSscript.resolve(props);
})
.fail(function(){
//console.log('FPS GET FAILED!');
//omniture
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName, props.omnitureValues.load);
FPSscript.reject();
});
})
.fail(function( jqxhr, settings, exception ) {
//console.log('FPS SCRIPT FAILED TO LOAD!');
//omniture
props.omnitureValues.load.cmTemplate = 'new';
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName, props.omnitureValues.load);
FPSscript.reject();
});
} else {
FPSscript.reject();
}
} else {
/*console.log('SCRIPT NOT LOADED TWICE!');*/
}
return FPSscript.promise(); // allows you to check the state if the promise and apply event handlers to it
},
getPersonalisedData: function(){
getPersonalisedData().done(function(){
FPSscript.resolve(props);
return props.fpsReturnedData;
})
.getPersonalisedData().fail(function(){
return false;
});
}
};
});
define('lightbox/global.ux.lightbox',['jquery'], function($) {
var props = {
'lightboxContainer' : null,
'lightboxElements' : null
};
var captureLightboxElements = function(){
//console.log('FUNCTION:captureLightboxElements');
props.lightboxContainer = $('#gux-lightbox');
props.lightboxElements = $('.lightbox-link');
};
var setupLightbox = function(){
//console.log('FUNCTION:setupLightbox');
$('#lightbox-close').on('click', function(e){
e.preventDefault();
props.lightboxContainer.hide();
});
props.lightboxElements.each(function(){
var elementLightboxID,
lightboxHTML,
$this = $(this);
$this.on('click', function(e){
e.preventDefault();
elementLightboxID = $this.data('load-lightbox');
lightboxHTML = $('#'+elementLightboxID)[0].outerHTML;
props.lightboxContainer.find('.content').empty();
props.lightboxContainer.find('.content').append(lightboxHTML);
props.lightboxContainer.show();
$('#details-show').on('click', function(e){
e.preventDefault();
$(this).closest('.caption').toggleClass('active');
});
});
});
};
return {
init: function() {
//console.log('GUX LIGHTBOX MODULE LOADED!');
captureLightboxElements();
setupLightbox();
}
};
});
define('responsive/global.ux.imageInterchange',['jquery'], function($) {
/* matchmedia not supported in IE9, need to get support for it, look into respond.js etc */
var props = {
'mediaWidths' : {
'mobile' : '(max-width: 767px)',
'desktop' : '(min-width: 768px)',
'mobileStatus' : true
},
'matchMedia' : {
'mobile' : null,
'desktop' : null
},
'img' : {
'elements' : null,
'mobile' : null,
'desktop' : null,
'imgData' : []
}
};
var setMatchMediaValues = function(){
//console.log('FUNCTION:setMatchMediaValues');
props.matchMedia.mobile = window.matchMedia(props.mediaWidths.mobile);
props.matchMedia.desktop = window.matchMedia(props.mediaWidths.desktop);
};
var handleMatchMedia = function (mediaQuery) {
if (mediaQuery.matches) {
//load mobile
props.mediaWidths.mobileStatus = true;
switchMediaImage();
} else {
//load desktop
props.mediaWidths.mobileStatus = false;
switchMediaImage();
}
};
var imageInterchangeSetup = function(){
//console.log('FUNCTION:imageInterchangeSetup');
handleMatchMedia(props.matchMedia.mobile);
props.matchMedia.mobile.addListener(handleMatchMedia);
};
var switchMediaImage = function(){
//console.log('FUNCTION:switchMediaImage');
for (var i = 0; i < props.img.elements.length; i++){
if (props.mediaWidths.mobileStatus === true){
props.img.elements.eq(i).attr('src', props.img.imgData[i].mobileImgSrc);
props.img.elements.eq(i).attr('data-interchange-size', props.img.imgData[i].desktopImgSize);
props.img.elements.eq(i).attr('data-interchange-img-src', props.img.imgData[i].desktopImgSrc);
} else if (props.mediaWidths.mobileStatus === false){
props.img.elements.eq(i).attr('src', props.img.imgData[i].desktopImgSrc);
props.img.elements.eq(i).attr('data-interchange-size', props.img.imgData[i].mobileImgSize);
props.img.elements.eq(i).attr('data-interchange-img-src', props.img.imgData[i].mobileImgSrc);
}
}
};
var captureImageData = function(){
//console.log('FUNCTION:captureImageData');
props.img.elements = '';
props.img.elements = $('body').find('.img-interchange');
var $this,
interchangeDataImgSrc = '',
interchangeDataImgSize = '',
mobileImgSrc = '',
mobileImgSize = 'small',
desktopImgSrc = '',
desktopImgSize = 'large';
props.img.imgData = [];
props.img.elements.each(function(index){
$this = $(this);
interchangeDataImgSrc = $this.attr('data-interchange-img-src');
interchangeDataImgSize = $this.attr('data-interchange-size');
if (interchangeDataImgSize == mobileImgSize){
mobileImgSrc = interchangeDataImgSrc;
desktopImgSrc = $this.attr('src');
} else {
desktopImgSrc = interchangeDataImgSrc;
mobileImgSrc = $this.attr('src');
}
props.img.imgData.push(
{
'elementIndex' : index,
'mobileImgSrc' : mobileImgSrc,
'mobileImgSize' : mobileImgSize,
'desktopImgSrc' : desktopImgSrc,
'desktopImgSize' : desktopImgSize
}
);
});
};
return {
init: function() {
//console.log('GUX IMAGE INTERCHANGE MODULE LOADED!');
setMatchMediaValues();
captureImageData();
imageInterchangeSetup();
},
captureImageData: function(){
//console.log('PUBLIC FUNCTION:captureImageData');
captureImageData();
handleMatchMedia(props.matchMedia.mobile);
},
mobileStatus: function(){
return props.mediaWidths.mobileStatus;
}
};
});
define('header/globile.header',['jquery'], function($) {
var props = {};
var globileHeaderSetup = function(){
props.headerContainer = $('#globile_header');
props.navItems = props.headerContainer.find('#navitems');
props.headerContainer.find('ul').not(':first').addClass('hide');
props.largestHeightPos = 0;
var animOne = 'animStageOne',
animTwo = 'animStageTwo',
animThree = 'animStageThree',
largestMenuItemHeight,
headerBarHeight,
cookieTabHeight,
currentPageHeight,
totalHeight,
$tognavpanel,
$navItems,
$item;
props.headerContainer.find('#tognavpanel').css('display', 'block');
currentPageHeight = getCurrentPageHeight();
props.headerContainer.find('#navpanel').height(currentPageHeight);
props.headerContainer.find('#tognavpanel').on('click', function(e){
e.preventDefault();
$tognavpanel = $(this);
if ($tognavpanel.hasClass('active')) {
props.headerContainer.removeClass('navOpen');
$(this).removeClass('active').siblings('#navpanel').removeClass('navon').addClass('navoff');
$('#global-ux').css({'height':'auto', 'overflow':'auto'});
} else {
props.headerContainer.addClass('navOpen');
$(this).addClass('active').siblings('#navpanel').removeClass('navoff').addClass('navon');
currentPageHeight = getCurrentPageHeight();
headerBarHeight = props.headerContainer.find('#mastHead').outerHeight();
cookieTabHeight = $('#cookiePolicyWrapper').outerHeight();
largestMenuItemHeight = getLargestMenuItemHeight();
totalHeight = largestMenuItemHeight + headerBarHeight + cookieTabHeight;
props.headerContainer.find('#navpanel').height(currentPageHeight);
if (totalHeight > currentPageHeight){
$('#global-ux').css({'height':totalHeight, 'overflow':'hidden'})
.find('#navpanel').css({'height':totalHeight, 'overflow':'hidden'});
} else {
$('#global-ux').css({'height':currentPageHeight, 'overflow':'hidden'})
.find('#navpanel').css({'height':currentPageHeight, 'overflow':'hidden'});
}
}
});
props.navItems.find('a').on('click', function(){
$navItems = $(this);
$item = $navItems.closest('.item');
var valueID = parseInt($navItems.attr('id'), 10);
if ($navItems.hasClass('subcta')) {
if ($item.hasClass('one')){
$navItems.closest('.item').next('.two').children('ul').removeClass('show').addClass('hide').closest('.'+valueID).removeClass('hide').addClass('show');
if (props.navItems.hasClass(animOne)) {
props.navItems.addClass(animTwo).removeClass(animOne);
}
} else if ($item.hasClass('two')) {
$navItems.closest('.item').next('.three').children('ul').removeClass('show').addClass('hide').closest('.'+valueID).removeClass('hide').addClass('show');
if (props.navItems.hasClass(animTwo)) {
props.navItems.addClass(animThree).removeClass(animTwo);
}
}
}
else if ($navItems.hasClass('back')) {
if (props.navItems.hasClass(animTwo)) {
props.navItems.addClass(animOne).removeClass(animTwo);
} else if (props.navItems.hasClass(animThree)){
props.navItems.addClass(animTwo).removeClass(animThree);
}
}
largestMenuItemHeight = getLargestMenuItemHeight();
totalHeight = largestMenuItemHeight + headerBarHeight + cookieTabHeight;
props.headerContainer.find('#navpanel').height(currentPageHeight);
if (totalHeight > currentPageHeight){
$('#global-ux').css({'height':totalHeight, 'overflow':'hidden'})
.find('#navpanel').css({'height':totalHeight, 'overflow':'hidden'});
} else {
$('#global-ux').css({'height':currentPageHeight, 'overflow':'hidden'})
.find('#navpanel').css({'height':currentPageHeight, 'overflow':'hidden'});
}
});
};
var getCurrentPageHeight = function(){
var pageHeight = $('body').height();
return pageHeight;
};
var getLargestMenuItemHeight = function() {
var $item = props.headerContainer.find('#navitems .item');
var itemHeight = 0,
largestItemHeight = 0,
$this;
$item.each(function(){
$this = $(this);
itemHeight = $this.height();
if (itemHeight > largestItemHeight){ largestItemHeight = itemHeight; }
});
return largestItemHeight;
};
return {
init: function() {
//console.log('GLOBILE HEADER MODULE LOADED!');
globileHeaderSetup();
}
};
});
