define('campaign/campaign-revealer',['jquery', 'responsive/global.ux.imageInterchange'], function($, guxImageInterchange) {
var props = {
'container' : null,
'socialElements' : [],
'socialTabLimit' : 3,
'socialTabHeights' : [],
'mobileStatus' : null,
'omnitureValues' : {
'pageName' : '',
'data' : {
'deviceType' : 'mobile',
'cmTemplate' : 'unknown',
'cmAction' : '',
'cmContentType' : '',
'cmContentId' : ''
}
}
};
var campaignRevealSetup = function(){
//console.log('FUNCTION:campaignRevealSetup');
props.container = $('.social-items');
// omniture
props.omnitureValues.pageName = $('#global-ux').data('page');
props.omnitureValues.data.cmTemplate = GuxGlobals.omniturePersonalisedState;
props.omnitureValues.data.cmContentType = 'campaign';
mobileStatus();
props.socialTabHeights = getHeights();
/* set default height limit on load of the page */
setHeights();
var $this;
$(window).resize(function(){
mobileStatus();
responsiveHeights();
});
// loop over social-items
props.container.each(function(){
$this = $(this);
setupViewCloseHandler($this);
omnitureEventHandlerSetup($this);
});
};
var responsiveHeights = function(){
if (props.mobileStatus === true){
props.socialTabHeights = getHeights();
setHeights();
}
};
var setupViewCloseHandler = function($this){
var socialItemPos,
$viewCloseElement,
$socialItemsInner,
$clickedElement;
socialItemPos = $this.index();
$socialItemsInner = $this.find('.social-items-inner');
$this.find('.viewCloseTab a').each(function(){
$viewCloseElement = $(this);
$viewCloseElement.on('click', function(e){
e.preventDefault();
$clickedElement = $(this);
socialItemPos = $clickedElement.closest('.social-items').index();
$clickedElement.removeClass('active')
.siblings('a').addClass('active');
if ($clickedElement.hasClass('view-all')) {
props.container.eq(socialItemPos).find('.social-items-inner').height(props.socialTabHeights[socialItemPos].totalHeight);
} else if ($clickedElement.hasClass('view-less')) {
props.container.eq(socialItemPos).find('.social-items-inner').height(props.socialTabHeights[socialItemPos].heightLimit);
}
});
});
};
var getHeights = function(){
//console.log('FUNCTION:getHeights');
var $this,
$socialTabElements,
socialTabElementsLength,
currentHeight = 0,
headingHeight = 0,
viewCloseTabHeight = 0,
totalHeight,
heightLimit,
socialItemIndex,
socialTabLimit,
collectedHeights = [];
props.container.each(function(index){
socialItemIndex = index;
$this = $(this);
socialTabLimit = $this.attr('data-tab-limit');
totalHeight = 0;
heightLimit = 0;
headingHeight = props.container.find('.heading').outerHeight();
viewCloseTabHeight = props.container.find('.viewCloseTab').height();
$socialTabElements = $this.find('.social-tab');
socialTabElementsLength = $socialTabElements.length;
/* if tab limit doesn't exist or greater than actual elements, fallack to default value i.e. 3 */
if (socialTabLimit > ($socialTabElements.length - 1) || socialTabLimit === '') {
socialTabLimit = props.socialTabLimit;
}
$socialTabElements.each(function(index){
currentHeight = $(this).height();
if (index < socialTabLimit){
heightLimit += currentHeight;
}
totalHeight += currentHeight;
});
heightLimit = heightLimit + headingHeight + viewCloseTabHeight;
totalHeight = totalHeight + headingHeight + viewCloseTabHeight;
collectedHeights.push({
'heightLimit' : heightLimit,
'totalHeight' : totalHeight,
'socialItemIndex' : socialItemIndex
});
});
return collectedHeights;
};
var setHeights = function(){
var $socialItemsInner;
props.container.each(function(index){
$socialItemsInner = $(this).find('.social-items-inner');
if ($socialItemsInner.find('a.view-all').hasClass('active')){
/* set to total height if closed */
$socialItemsInner.height(props.socialTabHeights[index].heightLimit);
} else {
/* set to total height if open */
$socialItemsInner.height(props.socialTabHeights[index].totalHeight);
}
});
};
var omnitureEventHandlerSetup = function($this){
//console.log('FUNCTION:omnitureEventHandlerSetup');
var $elements,
socialTabPos,
$ctaLink;
$this.find('.social-tab').each(function(index){
$elements = $(this);
socialTabPos = index;
$elements.find('a').on('click', function(e){
$ctaLink = $(this);
props.omnitureValues.data.cmTemplate = GuxGlobals.omniturePersonalisedState;
props.omnitureValues.data.cmAction = 'campaign cta click';
props.omnitureValues.data.cmContentId = $ctaLink.find('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
});
});
};
var mobileStatus = function(){
props.mobileStatus = guxImageInterchange.mobileStatus();
//console.log('PROPS.MOBILESTATUS = ' + props.mobileStatus);
};
return {
init: function() {
//console.log('CAMPAIGN-REVEALER MODULE LOADED!');
campaignRevealSetup();
}
};
});
define('build-price/global.ux.build-price',['jquery'], function($) {
var props = {
'container' : null,
'omnitureValues' : {
'pageName' : '',
'data' : {
'deviceType' : 'mobile',
'cmTemplate' : 'unknown',
'cmAction' : '',
'cmContentType' : '',
'cmContentId' : ''
}
}
};
var buildPriceSetup = function(){
var $this,
$ctaLink;
// omniture
props.omnitureValues.pageName = $('#global-ux').data('page');
props.omnitureValues.data.cmTemplate = GuxGlobals.omniturePersonalisedState;
props.omnitureValues.data.cmContentType = 'build and price';
props.container = $('.buildprice');
props.container.each(function(){
$this = $(this);
//omniture cta click
$this.find('.heading a').on('click', function(e){
$ctaLink = $(this);
e.stopPropagation();
props.omnitureValues.data.cmTemplate = GuxGlobals.omniturePersonalisedState;
props.omnitureValues.data.cmAction = 'buildprice cta click';
props.omnitureValues.data.cmContentId = $ctaLink.closest('.buildprice-inner').find('img').attr('data-asset');
TK.broadcast(document.body, 'brand:'+props.omnitureValues.pageName+':content:on page', props.omnitureValues.data);
});
});
};
return {
init: function() {
//console.log('BUILD-PRICE MODULE LOADED!');
buildPriceSetup();
}
};
});
define('page-module/homepage-module',['jquery', 'common', 'jquery-cookie', 'respond', 'cookie/globile.cookie', 'cookie/global.ux.cookie', 'personalisation/global.ux.personalise', 'lightbox/global.ux.lightbox','responsive/global.ux.imageInterchange', 'header/globile.header', 'carousel/global.ux.carousel', 'showroom/global.ux.showroom', 'promotion/global.ux.promotion', 'campaign/campaign-revealer', 'build-price/global.ux.build-price'],
function($, common, jqueryCookie, respond, globileCookie, guxCookie, guxPersonalise, guxLightbox, guxImageInterchange, globileHeader, carousel, showroom, promotion, campaignRevealer, buildPrice) {
/* homepage module */
return {
init: function() {
GuxGlobals.carousel = {};
//console.log('HOMEPAGE MODULE  - LOADED!');
//omniture
if (GuxGlobals) {
GuxGlobals.omniturePersonalisedState = 'unknown';
}
common.accordians('#global-ux');
/*guxCookie.init();*/
globileCookie.init();
guxImageInterchange.init();
if ($('.gux-slider').length > 0) {
carousel.init();
GuxGlobals.carousel.stopAutoSlide = carousel.stopAutoSlide;
}
if($('#globile_header').length) {
globileHeader.init();
}
if($('#gux-lightbox').length) {
guxLightbox.init();
}
if($('.showroom').length) {
showroom.init();
}
if($('.promotion').length) {
promotion.init();
}
if($('.social-items').length) {
campaignRevealer.init();
}
if($('.buildprice').length) {
buildPrice.init();
}
}
};
});
