define('common',['jquery'], function($) {
function initCheckboxes() {
$('#global-ux').on('click', 'input[type=checkbox]', function(e) {
var checked = $(this).prop('checked');
if(checked) {
$(this).addClass('checked');
} else {
$(this).removeClass('checked');
}
});
}
function initAccordian() {
$('#global-ux').on('click', '.accordian > h5 ,.accordian > h3', function() {
var accordianContainer = $(this).parents('.accordian'),
accordianContent = accordianContainer.find('.accordian-content');
accordianContainer.siblings('.accordian').find('.accordian-content').slideUp('fast', function(param) {
$(this).parents('.accordian').removeClass('accordian-open');
});
if(accordianContainer.hasClass('accordian-open')) {
accordianContent.slideUp('fast', function() {
accordianContainer.removeClass('accordian-open');
});
} else {
accordianContent.slideDown('fast', function() {
accordianContainer.addClass('accordian-open');
});
}
});
}
function formatNumber(num, thousandSeparator, decimalSeparator) {
var integer = num.toString().split('.')[0].replace(',', ''),
fraction = num.toString().split('.')[1],
newNumber = '',
i,
j;
thousandSeparator = thousandSeparator || '';
decimalSeparator = decimalSeparator || '.';
for(i = integer.length - 1, j = 1; i >= 0; i--, j++) {
newNumber = integer[i] + newNumber;
if(j%3 === 0 && i !== 0) {
newNumber = thousandSeparator + newNumber;
}
}
if(fraction) {
newNumber += decimalSeparator + fraction;
}
return newNumber;
}
return {
checkboxes: initCheckboxes,
accordians: initAccordian,
formatNumber: formatNumber
};
});
