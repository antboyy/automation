font-family: 'FordAntennaLight', arial;
color: #1b394e;
margin: 0; }
#global-ux p {
margin: 0 0 1.25em;
font-size: 0.875rem;
/* font size based on 16px : 14px; */
line-height: 1.25rem;
/* line height based on 20px for font-size: 14px; (1.428em) */ }
#global-ux a.custom-button {
color: #fff;
letter-spacing: 1px; }
#global-ux p a, #global-ux li a {
text-decoration: underline; }
#global-ux .chevron, #global-ux .button,
#global-ux .tel {
text-decoration: none; }
