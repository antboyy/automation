font-family: 'FordAntennaLight'; }
/*#global-ux a:hover{
color: #0096c8;
}*/
/* ====================
CONVERT PX to REMS: http://offroadcode.com/prototypes/rem-calculator/
Base on 16px base font so 1em is 16px.
8px = 0.5em
9px = 0.5625em
10px = 0.625em
11px = 0.6875em
12px = 0.75em
13px = 0.8125em
14px = 0.875em
15px = 0.9375em
16px = 1em (base)
18px = 1.125em
20px = 1.25em
22px = 1.375em
24px = 1.5em
26px = 1.625em
28px = 1.75em
30px = 1.875em
32px = 2em
34px = 2.125em
36px = 2.25em
38px = 2.375em
40px = 2.5em
==================== */
@font-face {
font-family: "FordAntennaRE";
src: url("/cs/globalux/font/ford/fordantenna-re-webfont.eot");
src: url("/cs/globalux/font/ford/fordantenna-re-webfont.eot?") format("embedded-opentype"), url("/cs/globalux/font/ford/fordantenna-re-webfont.woff") format("woff"), url("/cs/globalux/font/ford/fordantenna-re-webfont.ttf") format("truetype"), url("/cs/globalux/font/ford/fordantenna-re-webfont.svg#web") format("svg");
font-style: normal;
font-weight: normal; }
@font-face {
